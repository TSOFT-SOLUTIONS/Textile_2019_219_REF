    Private Sub ConsumedYarnPavu_Calculation(ByVal RwNo As Integer)
        Dim vTotMtrs As String = 0
        Dim ConsYarn As String = 0
        Dim PavuConsMtrs As String = 0
        Dim vProdMtrs
        Dim Clt_Id As Integer = 0

        Dim cloth_pick As String = 0
        Dim cloth_width As String = 0
        Dim Stk As String = ""
        Dim RdSp As String = 0
        Dim vMultiplyr As Integer = 0
        Dim ConsThri As String = 0

        If FrmLdSTS = True Or NoCalc_Status = True Then Exit Sub

        If dgv_Details.Rows.Count <= 0 Then Exit Sub
        If RwNo < 0 Or RwNo > dgv_Details.Rows.Count - 1 Then Exit Sub



        With dgv_Details
            vTotMtrs = 0
            If dgv_Details.RowCount > 0 Then
                vTotMtrs = Val(dgv_Details.Rows(dgv_Details.Rows(RwNo).Cells(12).Value))
            End If



            Clt_Id = Common_Procedures.Cloth_NameToIdNo(con, dgv_Details.Rows(RwNo).Cells(3).Value)

            If Val(Clt_Id) <> 0 Then

                vProdMtrs = Format(Val(vTotMtrs), "#########0.00")
                If txt_Folding_perc.Visible = True Then

                    If Val(txt_Folding_perc.Text) <> 0 Then
                        vProdMtrs = Format((Val(vTotMtrs) * Val(txt_Folding_perc.Text)) / 100, "#########0.00")
                    Else
                        vProdMtrs = Format(Val(vTotMtrs), "#########0.00")
                    End If

                End If

                '---*************** Pavu Consumption **************

                PavuConsMtrs = Common_Procedures.get_Pavu_Consumption(con, Clt_Id, 0, Val(vProdMtrs), Trim(dgv_Details.Rows(RwNo).Cells(28).Value), , , , True)
                txt_PavuConsum.Text = Format(Val(PavuConsMtrs), "#########0.00")
                dgv_Details.Rows(RwNo).Cells(27).Value = Format(Val(PavuConsMtrs), "#########0.00")


                '---*************** Yarn  Consumption **************

                Stk = (Common_Procedures.get_FieldValue(con, "Cloth_Head", "Stock_In", "(Cloth_Idno = '" & Val(Clt_Id) & "')"))
                'If Val(Weft) = 0 Then
                '    Weft = Val(.Rows(i).Cells(5).Value)
                'End If


                cloth_pick = Common_Procedures.get_FieldValue(con, "Cloth_Head", "Cloth_Pick", "(Cloth_Idno = '" & Val(Clt_Id) & "')")  'Val(.Rows(i).Cells(2).Value)
                cloth_width = Common_Procedures.get_FieldValue(con, "Cloth_Head", "Cloth_Width", "(Cloth_Idno = '" & Val(Clt_Id) & "')")

                RdSp = 2.5

                If Trim(UCase(Stk)) = "PCS" Then
                    vMultiplyr = 100
                Else
                    vMultiplyr = 110

                End If

                ConsYarn = Format(Val(cloth_pick * (Val(cloth_width) + Val(RdSp)) * Val(vMultiplyr) / 840), "############0.00000")
                ConsYarn = Math.Floor(Val(ConsYarn))
                ConsYarn = Format(Val(ConsYarn) / 1000, "############0.000")
                ConsThri = Format(Val(vProdMtrs * Val(ConsYarn)), "############0.0")

                'ConsYarn = Common_Procedures.get_Weft_ConsumedYarn(con, Clt_Id, Val(vTotMtrs))
                txt_YarnConsum.Text = Format(Val(ConsThri), "#########0.000")
                dgv_Details.Rows(RwNo).Cells(26).Value = Format(Val(ConsThri), "#########0.000")



            End If
        End With

    End Sub

