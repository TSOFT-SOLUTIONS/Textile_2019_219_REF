    Private Sub btn_Close_Selection_Click(ByVal sender As Object, ByVal e As System.EventArgs) Handles btn_Close_Selection.Click
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim i As Integer, n As Integer
        Dim sno As Integer = 0
        Dim Clo_IdNo As Integer = 0

        dgv_Details.Rows.Clear()

        NoCalc_Status = True

        sno = 0
        Clo_IdNo = 0

        For i = 0 To dgv_Selection.RowCount - 1

            If Val(dgv_Selection.Rows(i).Cells(7).Value) = 1 Then

                If Val(Clo_IdNo) = 0 Then Clo_IdNo = Common_Procedures.Cloth_NameToIdNo(con, dgv_Selection.Rows(i).Cells(3).Value)

                n = dgv_Details.Rows.Add()

                sno = sno + 1
                dgv_Details.Rows(n).Cells(0).Value = Val(sno)
                dgv_Details.Rows(n).Cells(1).Value = dgv_Selection.Rows(i).Cells(1).Value
                dgv_Details.Rows(n).Cells(2).Value = dgv_Selection.Rows(i).Cells(2).Value
                dgv_Details.Rows(n).Cells(3).Value = dgv_Selection.Rows(i).Cells(3).Value
                If Val(dgv_Selection.Rows(i).Cells(4).Value) <> 0 Then dgv_Details.Rows(n).Cells(4).Value = Val(dgv_Selection.Rows(i).Cells(4).Value)
                If Val(dgv_Selection.Rows(i).Cells(5).Value) <> 0 Then dgv_Details.Rows(n).Cells(5).Value = Format(Val(dgv_Selection.Rows(i).Cells(5).Value), "#########0.00")
                If Val(dgv_Selection.Rows(i).Cells(6).Value) <> 0 Then dgv_Details.Rows(n).Cells(6).Value = Format(Val(dgv_Selection.Rows(i).Cells(6).Value), "#########0.00")
                If Val(dgv_Selection.Rows(i).Cells(9).Value) <> 0 Then dgv_Details.Rows(n).Cells(7).Value = Format(Val(dgv_Selection.Rows(i).Cells(9).Value), "#########0.00")
                If Val(dgv_Selection.Rows(i).Cells(10).Value) <> 0 Then dgv_Details.Rows(n).Cells(8).Value = Format(Val(dgv_Selection.Rows(i).Cells(10).Value), "#########0.00")
                If Val(dgv_Selection.Rows(i).Cells(11).Value) <> 0 Then dgv_Details.Rows(n).Cells(9).Value = Format(Val(dgv_Selection.Rows(i).Cells(11).Value), "#########0.00")
                If Val(dgv_Selection.Rows(i).Cells(12).Value) <> 0 Then dgv_Details.Rows(n).Cells(10).Value = Format(Val(dgv_Selection.Rows(i).Cells(12).Value), "#########0.00")
                If Val(dgv_Selection.Rows(i).Cells(13).Value) <> 0 Then dgv_Details.Rows(n).Cells(11).Value = Format(Val(dgv_Selection.Rows(i).Cells(13).Value), "#########0.00")
                If Val(dgv_Selection.Rows(i).Cells(14).Value) <> 0 Then dgv_Details.Rows(n).Cells(12).Value = Format(Val(dgv_Selection.Rows(i).Cells(14).Value), "#########0.00")
                dgv_Details.Rows(n).Cells(13).Value = dgv_Selection.Rows(i).Cells(8).Value

            End If

        Next i

        NoCalc_Status = False

        If New_Entry = True Then
            If Val(txt_Rate_Type1.Text) = 0 And Val(txt_Rate_Type2.Text) = 0 And Val(txt_Rate_Type3.Text) = 0 And Val(txt_Rate_Type4.Text) = 0 And Val(txt_Rate_Type5.Text) = 0 Then

                Da = New SqlClient.SqlDataAdapter("select a.* from Cloth_Head a Where a.cloth_Idno = " & Str(Val(Clo_IdNo)), con)
                Dt1 = New DataTable
                Da.Fill(Dt1)
                If Dt1.Rows.Count > 0 Then
                    txt_Rate_Type1.Text = Dt1.Rows(0).Item("Wages_For_Type1").ToString
                    If Val(txt_Rate_Type1.Text) = 0 Then txt_Rate_Type1.Text = ""

                    txt_Rate_Type2.Text = Dt1.Rows(0).Item("Wages_For_Type2").ToString
                    If Val(txt_Rate_Type2.Text) = 0 Then txt_Rate_Type2.Text = ""

                    txt_Rate_Type3.Text = Dt1.Rows(0).Item("Wages_For_Type3").ToString
                    If Val(txt_Rate_Type3.Text) = 0 Then txt_Rate_Type3.Text = ""

                    txt_Rate_Type4.Text = Dt1.Rows(0).Item("Wages_For_Type4").ToString
                    If Val(txt_Rate_Type4.Text) = 0 Then txt_Rate_Type4.Text = ""

                    txt_Rate_Type5.Text = Dt1.Rows(0).Item("Wages_For_Type5").ToString
                    If Val(txt_Rate_Type5.Text) = 0 Then txt_Rate_Type5.Text = ""

                End If

            End If
        End If

        Total_Calculation()

        Grid_Cell_DeSelect()

        pnl_Back.Enabled = True
