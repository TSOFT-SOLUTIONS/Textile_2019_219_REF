            If e.ColumnIndex = 3 Then

                If cbo_Grid_ClothName.Visible = False And Trim(UCase(cbo_Type.Text)) <> "DELIVERY" Or Val(cbo_Grid_ClothName.Tag) <> e.RowIndex Then

