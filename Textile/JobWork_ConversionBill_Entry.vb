                                End If


                                vFLD_PERC = Format(100 - Val(prn_HdDt.Rows(0).Item("Folding_Perc").ToString), "##########0.00")
                                vFLD_LS_MTR = Format(Val(prn_DetDt.Rows(prn_DetIndx).Item("Type1_Folding_Less_Meters").ToString), "#########0.00")
                                vNET_MTR = Format(Val(prn_DetDt.Rows(prn_DetIndx).Item("Type1_Net_Meters").ToString), "#########0.00")

