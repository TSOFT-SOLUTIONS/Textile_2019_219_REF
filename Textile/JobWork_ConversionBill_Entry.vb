        Dim NewCode As String = ""
        Dim Led_ID As Integer = 0
        Dim SalAc_ID As Integer = Common_Procedures.CommonLedger.Conversion_Bill_Charges_Ac  ' 18
        Dim Clo_ID As Integer = 0
