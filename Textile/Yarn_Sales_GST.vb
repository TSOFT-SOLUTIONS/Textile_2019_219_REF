    Private Sub txt_TdsPerc_KeyDown(sender As Object, e As KeyEventArgs) Handles txt_TdsPerc.KeyDown
        If e.KeyValue = 38 Then
            txt_AddLess_AfterTax.Focus()
        End If

        If e.KeyValue = 40 Then
            If txt_TdsAmount.Enabled And txt_TdsAmount.Visible Then
                txt_TdsAmount.Focus()

            Else
                If txt_Remarks.Enabled And txt_Remarks.Visible Then txt_Remarks.Focus()
            End If
        End If
    End Sub

    Private Sub txt_TdsPerc_KeyPress(sender As Object, e As KeyPressEventArgs) Handles txt_TdsPerc.KeyPress

        If Common_Procedures.Accept_NumericOnly(Asc(e.KeyChar)) = 0 Then e.Handled = True
        If Asc(e.KeyChar) = 13 Then
            If txt_TdsAmount.Enabled And txt_TdsAmount.Visible Then
                txt_TdsAmount.Focus()

            Else
                If txt_Remarks.Enabled And txt_Remarks.Visible Then txt_Remarks.Focus()
            End If

        End If
