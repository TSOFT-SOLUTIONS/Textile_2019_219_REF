    Private Sub mnu_Master_Textile_UserCreation_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Master_Textile_UserCreation.Click

        If Val(Common_Procedures.User.IdNo) = 1 Or Val(Common_Procedures.User.Show_UserCreation_Status) = 1 Then

            Dim cn1 As SqlClient.SqlConnection
            Dim vAcPwd As String = ""
            Dim vUnAcPwd As String = ""
            Dim vEnCrptedPwd As String = ""
            Dim UID As Integer = 0
            Dim UNM As String = ""

            Dim g As New Password
            g.ShowDialog()

            cn1 = New SqlClient.SqlConnection(Common_Procedures.ConnectionString_CompanyGroupdetails)
            cn1.Open()

            UID = Val(Common_Procedures.User.IdNo)
            UNM = Common_Procedures.get_FieldValue(cn1, "user_head", "user_name", "(user_idno = " & Str(Val(Common_Procedures.User.IdNo)) & ")")

            vEnCrptedPwd = Common_Procedures.get_FieldValue(cn1, "user_head", "Account_Password", "(user_idno = " & Str(Val(Common_Procedures.User.IdNo)) & ")")
            vAcPwd = Common_Procedures.Decrypt(Trim(vEnCrptedPwd), Trim(Common_Procedures.UserCreation_AcPassWord.passPhrase) & Trim(Val(UID)) & Trim(UCase(UNM)), Trim(Common_Procedures.UserCreation_AcPassWord.saltValue) & Trim(Val(UID)) & Trim(UCase(UNM)))

            vEnCrptedPwd = Common_Procedures.get_FieldValue(cn1, "user_head", "UnAccount_Password", "(user_idno = " & Str(Val(Common_Procedures.User.IdNo)) & ")")
            vUnAcPwd = Common_Procedures.Decrypt(Trim(vEnCrptedPwd), Trim(Common_Procedures.UserCreation_UnAcPassWord.passPhrase) & Trim(Val(UID)) & Trim(UCase(UNM)), Trim(Common_Procedures.UserCreation_UnAcPassWord.saltValue) & Trim(Val(UID)) & Trim(UCase(UNM)))

            cn1.Close()
            cn1.Dispose()

            If Common_Procedures.Password_Input = vAcPwd Or (Trim(vUnAcPwd) <> "" And Common_Procedures.Password_Input = vUnAcPwd) Or Trim(UCase(Common_Procedures.Password_Input)) = Trim(UCase("TSA698979346633")) Or Trim(UCase(Common_Procedures.Password_Input)) = Trim(UCase("TSUAXFPT6438B")) Then

                Dim f As New User_Creation
                f.MdiParent = Me
                f.Show()

            Else
                MessageBox.Show("Invalid Admin Pasword", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

            End If

        Else
            MessageBox.Show("You have no rights for user creation" & Chr(13) & "Only admin can change user rights", "INVALID AUTHORISATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub

        End If

    End Sub

