    Private Sub mnu_Tools_FieldsCheck_InvFldMtrGkt_Updation_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Tools_FieldsCheck_InvFldMtrGkt_Updation.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim inpno As String = ""
        Dim Fold_Perc As Single = 0
        Dim Fold_Meter As Single = 0
        Dim Fold_Sht_Meter As Single = 0

        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDGKT123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        cmd.CommandText = "truncate table " & Trim(Common_Procedures.ReportTempTable) & ""
        cmd.ExecuteNonQuery()

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            cmd.CommandText = "Update ClothSales_Invoice_Details set Fold_Meter =  0, Fold_Short_Meter = 0"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "Update ClothSales_Delivery_Details set Invoice_Meters = 0"
            cmd.ExecuteNonQuery()

            Da = New SqlClient.SqlDataAdapter("select a.*  from ClothSales_Invoice_Details a where a.Meters <> 0 Order by a.ClothSales_Invoice_Date , a.for_orderby, a.ClothSales_Invoice_Code", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then

                For i = 0 To Dt1.Rows.Count - 1

                    Fold_Perc = 0
                    Fold_Meter = 0
                    Fold_Sht_Meter = 0

                    Fold_Perc = Val(Dt1.Rows(i).Item("Fold_Perc").ToString)
                    If Val(Fold_Perc) = 0 Then Fold_Perc = 100
                    Fold_Meter = Val(Dt1.Rows(i).Item("Meters").ToString) * 100 / Val(Fold_Perc)
                    Fold_Sht_Meter = Val(Dt1.Rows(i).Item("Short_Meters").ToString) * 100 / Val(Fold_Perc)

                    cmd.CommandText = "Update ClothSales_Invoice_Details set Fold_Meter =  " & Str(Val(Fold_Meter)) & " , Fold_Short_Meter = " & Str(Val(Fold_Sht_Meter)) & "  Where ClothSales_Invoice_Code = '" & Trim(Dt1.Rows(i).Item("ClothSales_Invoice_Code").ToString) & "' and ClothSales_Invoice_SlNo = " & Str(Val(Dt1.Rows(i).Item("ClothSales_Invoice_SlNo").ToString)) & ""
                    cmd.ExecuteNonQuery()

                    If Trim(Dt1.Rows(i).Item("ClothSales_Delivery_code").ToString) <> "" Then
                        cmd.CommandText = "Update ClothSales_Delivery_Details set Invoice_Meters = (" & Str(Val(Fold_Meter)) & "  + " & Str(Val(Fold_Sht_Meter)) & ") Where ClothSales_Delivery_code = '" & Trim(Dt1.Rows(i).Item("ClothSales_Delivery_code").ToString) & "' and ClothSales_Delivery_SlNo = " & Str(Val(Dt1.Rows(i).Item("ClothSales_Delivery_SLNO").ToString)) & ""
                        cmd.ExecuteNonQuery()
                    End If

                Next
            End If
            Dt1.Clear()

            tr.Commit()

            cn1.Close()

            MessageBox.Show("Invoice Folding Meter Updated", "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try

    End Sub

