        Dim g As New Password
        g.ShowDialog()

        Dim PWD As String = ""
        Dim PWD_R As String = Format(Now, "ddMMyy").Trim

        For i As Integer = Len(PWD_R) To 1 Step -1
            PWD = PWD + Mid(PWD_R, i, 1)
        Next
        PWD = "TS" & Trim(PWD)


        If Trim(UCase(Common_Procedures.Password_Input)) <> Trim(UCase(PWD)) Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

