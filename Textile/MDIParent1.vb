                For i = 0 To Dt1.Rows.Count - 1

                    RcptWgt = Val(Dt1.Rows(i).Item("Weight").ToString) - Val(Dt1.Rows(i).Item("Excess_Short").ToString) + Val(Dt1.Rows(i).Item("Waste_Tolerance_Weight").ToString)

                    cmd.CommandText = "Update Rewinding_Delivery_Details Set " &
                                        " Delivery_Weight = Delivery_Weight  + " & Str(Format(Val(RcptWgt), "#########0.000")) & " " &
                                        " Where Rewinding_Delivery_Code = '" & Trim(Dt1.Rows(i).Item("Rewinding_Delivery_Code").ToString) & "' and " &
                                            " Rewinding_Delivery_SlNo = " & Str(Val(Dt1.Rows(i).Item("Rewinding_Delivery_SlNo").ToString)) & " and " &
                                            " Set_No = '" & Trim(Dt1.Rows(i).Item("Set_No").ToString) & "' "
                    Nr = cmd.ExecuteNonQuery

                    cmd.CommandText = "Update Rewinding_Receipt_Head Set " &
                                        " Receipt_Type = 'SELECTION' " &
                                        " Where Rewinding_Receipt_Code = '" & Trim(Dt1.Rows(i).Item("Rewinding_Receipt_Code").ToString) & "' "
                    Nr = cmd.ExecuteNonQuery

                Next

            End If
            Dt1.Clear()

            tr.Commit()

            cn1.Close()

            MessageBox.Show("Rewinding Delivery Selection Details Updated", "FOR REWINDING DELIVERY UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try

    End Sub

