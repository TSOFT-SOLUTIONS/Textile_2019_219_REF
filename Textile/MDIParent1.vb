    Private Sub mnu_Tools_FieldsCheck_Loom_Opening_Stock_Deletion_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Tools_FieldsCheck_Loom_Opening_Stock_Deletion.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction

        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDKMT123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED.....", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            cmd.CommandText = "Delete from Stock_Pavu_Processing_Details Where Reference_Code LIKE 'LOMOP-%'"
            cmd.ExecuteNonQuery()

            tr.Commit()

            cn1.Close()

            MessageBox.Show("Loo Opening Stock Successfully Updated", "FOR FIELDS UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR FIELDS UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            cmd.Dispose()
            tr.Dispose()

        End Try

    End Sub

    Private Sub mnu_tools_Field_Check_AgentCommission_Deletion_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_tools_Field_Check_AgentCommission_Deletion.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim da As New SqlClient.SqlDataAdapter
        Dim da1 As New SqlClient.SqlDataAdapter
        Dim cmd As New SqlClient.SqlCommand
        Dim dt As New DataTable
        Dim dt1 As New DataTable
        Dim Pk_Condition As String = ""
        Dim Nr As Integer = 0
        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDKMT123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED.....", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        Pk_Condition = "ICINV-"

        cmd.Connection = cn1


        Try

            da = New SqlClient.SqlDataAdapter("select Reference_Code from AgentCommission_Processing_Details where Reference_Code like '" & Trim(Pk_Condition) & "%' ", cn1)
            da.Fill(dt)
            If dt.Rows.Count > 0 Then
                For i = 0 To dt.Rows.Count - 1

                    If IsDBNull(dt.Rows(i).Item("Reference_Code").ToString) = False Then

                        da1 = New SqlClient.SqlDataAdapter("select Ic_Invoice_Code from Ic_Invoice_Head where Ic_Invoice_Code  = '" & Trim(Replace(dt.Rows(i).Item("Reference_Code").ToString, Trim(Pk_Condition), "")) & "'", cn1)
                        da1.Fill(dt1)
                        If dt1.Rows.Count = 0 Then
                            Nr = 0
                            cmd.CommandText = "Delete from AgentCommission_Processing_Details Where Reference_Code = '" & Trim(dt.Rows(i).Item("Reference_Code").ToString) & "'"
                            Nr = cmd.ExecuteNonQuery()
                        End If
                        dt1.Clear()
                        da1.Dispose()
                    End If

                Next

            End If
            dt.Clear()
            da.Dispose()
            cn1.Close()

            MessageBox.Show("Agent Cloth Commission Successfully Updated", "FOR FIELDS UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            MessageBox.Show(ex.Message, "FOR  MOVING...", MessageBoxButtons.OK, MessageBoxIcon.Error)

        Finally
            dt.Dispose()
            da.Dispose()

        End Try

    End Sub

