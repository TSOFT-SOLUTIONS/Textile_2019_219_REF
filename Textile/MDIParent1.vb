    Private Sub mnu_Tools_FieldsCheck_Processing_Fabric_Inspection_Updation_Click(sender As Object, e As EventArgs) Handles mnu_Tools_FieldsCheck_Processing_Fabric_Inspection_Updation.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim Nr As Long = 0
        Dim vINS_PCS As String, vINS_MTR As String, vINS_WGT As String
        Dim vREC_CODE As String, vREC_DETSLNO As Long
        Dim vENTRY_CODE As String

        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLD123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        tr = cn1.BeginTransaction

        Dim Sur = ""
        Dim Item_Main_Name = ""
        Dim vSQL_QUERY = ""




        Try

            cmd.Transaction = tr

            cmd.CommandText = "Update Textile_Processing_Receipt_Details set Processed_Fabric_inspection_Code = '', Inspection_Meters = 0, Inspection_Pcs = 0,  Inspection_Weight = 0 "
            cmd.ExecuteNonQuery()

            cmd.CommandText = "select * from Textile_Processing_Receipt_Details Where Receipt_Meters > 0 order by Cloth_Processing_Receipt_Code , Cloth_Processing_Receipt_Slno"
            Da = New SqlClient.SqlDataAdapter(cmd)
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then

                For I = 0 To Dt1.Rows.Count - 1

                    vREC_CODE = Trim(Dt1.Rows(I).Item("Cloth_Processing_Receipt_Code").ToString)
                    vREC_DETSLNO = Val(Dt1.Rows(I).Item("Cloth_Processing_Receipt_Slno").ToString)

                    vINS_PCS = 0
                    vINS_MTR = 0
                    vINS_WGT = 0
                    Common_Procedures.get_ProcessedFabric_inspection_Details(cn1, vREC_CODE, vREC_DETSLNO, vINS_PCS, vINS_MTR, vINS_WGT, tr)

                    cmd.CommandText = "Update Textile_Processing_Receipt_Details Set Inspection_Meters = " & Str(Val(vINS_MTR)) & " , Inspection_Pcs = " & Str(Val(vINS_PCS)) & " ,Inspection_Weight = " & Str(Val(vINS_WGT)) & " where Cloth_Processing_Receipt_Code = '" & Trim(vREC_CODE) & "' and Cloth_Processing_Receipt_Slno = " & Str(Val(vREC_DETSLNO))
                    Nr = cmd.ExecuteNonQuery()

                Next


            End If


            tr.Commit()

            cn1.Close()

            MessageBox.Show("Processing Fabric Inspection Updated", "FOR FIEDS CHECK...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR FIELDS CHECK UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try



    End Sub
