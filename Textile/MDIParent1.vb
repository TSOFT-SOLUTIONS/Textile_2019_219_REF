                                'Throw New ApplicationException("Invalid Weaver_ClothReceipt_Piece_Details Updation - " & vNewRollCode)
                                'Exit Sub
                            End If
                        End If

                    ElseIf Val(Dt1.Rows(i).Item("ClothType_IdNo").ToString) = 4 Then
                        Nr = 0
                        cmd.CommandText = "Update Weaver_ClothReceipt_Piece_Details set PackingSlip_Code_Type4 = '" & Trim(vNewRollCode) & "', PackingSlip_Inc_Type1 = 1  Where lot_code = '" & Trim(Dt1.Rows(i).Item("Lot_Code").ToString) & "' and Piece_No = '" & Trim(Dt1.Rows(i).Item("Pcs_No").ToString) & "' and Type4_Meters <> 0 and PackingSlip_Code_Type4 = '" & Trim(vOldRollCode) & "'"
                        Nr = cmd.ExecuteNonQuery()
                        If Nr <> 1 Then
                            Nr = 0
                            cmd.CommandText = "Update Weaver_ClothReceipt_Piece_Details set PackingSlip_Code_Type4 = '" & Trim(vNewRollCode) & "', PackingSlip_Inc_Type1 = 1  Where lot_code = '" & Trim(Dt1.Rows(i).Item("Lot_Code").ToString) & "' and Piece_No = '" & Trim(Dt1.Rows(i).Item("Pcs_No").ToString) & "' and Type4_Meters <> 0 and PackingSlip_Code_Type4 = ''"
                            Nr = cmd.ExecuteNonQuery()
                            If Nr <> 1 Then
                                'Throw New ApplicationException("Invalid Weaver_ClothReceipt_Piece_Details Updation - " & vNewRollCode)
                                'Exit Sub
                            End If
                        End If

                    ElseIf Val(Dt1.Rows(i).Item("ClothType_IdNo").ToString) = 5 Then
                        Nr = 0
                        cmd.CommandText = "Update Weaver_ClothReceipt_Piece_Details set PackingSlip_Code_Type5 = '" & Trim(vNewRollCode) & "', PackingSlip_Inc_Type1 = 1 Where lot_code = '" & Trim(Dt1.Rows(i).Item("Lot_Code").ToString) & "' and Piece_No = '" & Trim(Dt1.Rows(i).Item("Pcs_No").ToString) & "' and Type5_Meters <> 0 and PackingSlip_Code_Type5 = '" & Trim(vOldRollCode) & "'"
                        Nr = cmd.ExecuteNonQuery()
                        If Nr <> 1 Then
                            Nr = 0
                            cmd.CommandText = "Update Weaver_ClothReceipt_Piece_Details set PackingSlip_Code_Type5 = '" & Trim(vNewRollCode) & "', PackingSlip_Inc_Type1 = 1 Where lot_code = '" & Trim(Dt1.Rows(i).Item("Lot_Code").ToString) & "' and Piece_No = '" & Trim(Dt1.Rows(i).Item("Pcs_No").ToString) & "' and Type5_Meters <> 0 and PackingSlip_Code_Type5 = ''"
                            Nr = cmd.ExecuteNonQuery()
                            If Nr <> 1 Then
                                'Throw New ApplicationException("Invalid Weaver_ClothReceipt_Piece_Details Updation - " & vNewRollCode)
                                'Exit Sub
                            End If
                        End If

                    End If

                Next

            End If
            Dt1.Clear()

            tr.Commit()

            cn1.Close()

            MessageBox.Show("Piece Excess/Short Codes Updated Successfully....", "FOR PIECE EXCESS/SHORT CODE UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR ROLL PACKING UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally
            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try

    End Sub

