    Private Sub mnu_Tools_Fields_Check_FPInvoice_OrderDetails_Updation_Jeno_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Tools_Fields_Check_FPInvoice_OrderDetails_Updation_Jeno.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim Da1 As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim inpno As String = ""
        Dim vFP_Invoice_Code As String = "", vFP_Invoice_No As String = ""
        Dim vComp_IdNo As Integer = 0
        Dim vLed_IdNo As Integer = 0, vSNo As Integer = 0, vFP_IdNo As Integer = 0
        Dim vFP_Order_No As String = "", vFP_Order_Code As String = ""
        Dim vQty As Single = 0, vOrdBy_No As Single = 0
        Dim I As Integer = 0
        Dim vPrev_InvCode As String = ""


        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "TSJ123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED....", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        Me.Cursor = Cursors.WaitCursor

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        cmd.CommandText = "truncate table FinishedProduct_Invoice_Order_Details"
        cmd.ExecuteNonQuery()

        Da1 = New SqlClient.SqlDataAdapter("select a.FinishedProduct_Invoice_Code, a.Company_IdNo, a.FinishedProduct_Invoice_No, a.for_OrderBy, a.FinishedProduct_Invoice_Date, a.Ledger_IdNo, b.FinishedProduct_IdNo, c.Processed_Item_Name, a.FinishedProduct_Order_Code, sum(b.Quantity) as InvQuantity from FinishedProduct_Invoice_Head a, FinishedProduct_Invoice_Details b, Processed_Item_Head c Where a.Selection_Type = 'ORDER' and a.FinishedProduct_Invoice_Code = b.FinishedProduct_Invoice_Code and a.Company_IdNo = b.Company_IdNo and a.FinishedProduct_Order_Code <> '' and b.FinishedProduct_IdNo = c.Processed_Item_IdNo group by a.FinishedProduct_Invoice_Code, a.Company_IdNo, a.FinishedProduct_Invoice_No, a.for_OrderBy, a.FinishedProduct_Invoice_Date, a.Ledger_IdNo, b.FinishedProduct_IdNo, c.Processed_Item_Name, a.FinishedProduct_Order_Code Having sum(b.Quantity) <> 0 Order by a.FinishedProduct_Invoice_Date, a.for_OrderBy, a.FinishedProduct_Invoice_Code, a.Company_IdNo, a.FinishedProduct_Invoice_No, a.Ledger_IdNo, c.Processed_Item_Name, b.FinishedProduct_IdNo, a.FinishedProduct_Order_Code", cn1)
        Dt1 = New DataTable
        Da1.Fill(Dt1)

        vPrev_InvCode = ""
        vSNo = 0
        If Dt1.Rows.Count > 0 Then
            For I = 0 To Dt1.Rows.Count - 1

                If vPrev_InvCode <> "" Then
                    If Trim(UCase(vPrev_InvCode)) <> Trim(UCase(Dt1.Rows(I).Item("FinishedProduct_Invoice_Code").ToString)) Then
                        vSNo = 0
                    End If
                End If

                cmd.Parameters.Clear()
                cmd.Parameters.AddWithValue("@Entry_Date", Dt1.Rows(I).Item("FinishedProduct_Invoice_Date"))

                vSNo = vSNo + 1
                vFP_Invoice_Code = Dt1.Rows(I).Item("FinishedProduct_Invoice_Code").ToString
                vComp_IdNo = Val(Dt1.Rows(I).Item("Company_IdNo").ToString)
                vFP_Invoice_No = Dt1.Rows(I).Item("FinishedProduct_Invoice_No").ToString
                vOrdBy_No = Val(Dt1.Rows(I).Item("for_OrderBy").ToString)
                vLed_IdNo = Val(Dt1.Rows(I).Item("Ledger_IdNo").ToString)
                vFP_IdNo = Val(Dt1.Rows(I).Item("FinishedProduct_IdNo").ToString)
                vQty = Val(Dt1.Rows(I).Item("InvQuantity").ToString)
                vFP_Order_Code = Dt1.Rows(I).Item("FinishedProduct_Order_Code").ToString
                vFP_Order_No = Microsoft.VisualBasic.Left(vFP_Order_Code, Len(vFP_Order_Code) - 6)

                cmd.CommandText = "Insert into FinishedProduct_Invoice_Order_Details (  FinishedProduct_Invoice_Code  ,          Company_IdNo       ,    FinishedProduct_Invoice_No ,         for_OrderBy         , FinishedProduct_Invoice_Date,         Ledger_IdNo         ,            Sl_No      , FinishedProduct_Order_No     ,     FinishedProduct_IdNo  ,         Quantity      ,    FinishedProduct_Order_Code   ) " &
                                    "          Values                                ('" & Trim(vFP_Invoice_Code) & "', " & Str(Val(vComp_IdNo)) & ", '" & Trim(vFP_Invoice_No) & "', " & Str(Val(vOrdBy_No)) & ",       @Entry_Date           ,  " & Str(Val(vLed_IdNo)) & ", " & Str(Val(vSNo)) & ",  '" & Trim(vFP_Order_No) & "', " & Str(Val(vFP_IdNo)) & ", " & Str(Val(vQty)) & ",  '" & Trim(vFP_Order_Code) & "' ) "
                cmd.ExecuteNonQuery()

                vPrev_InvCode = Dt1.Rows(I).Item("FinishedProduct_Invoice_Code").ToString

            Next I
        End If
        Dt1.Clear()

        cmd.Dispose()

        cn1.Close()
        cn1.Dispose()

        Me.Cursor = Cursors.Default

        MessageBox.Show("Fields Updated", "FOR FP INVOICE ORDER DETAILS UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

    End Sub

