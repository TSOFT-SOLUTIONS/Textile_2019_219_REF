    Private Sub mnu_Tools_FieldsCheck_VehicleNo_Updation_Click(sender As System.Object, e As System.EventArgs) Handles mnu_Tools_FieldsCheck_VehicleNo_Updation.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim Sno As Integer = 0
        Dim Nr As Long = 0
        Dim vVECH_No As String = ""

        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLD123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            Da = New SqlClient.SqlDataAdapter("Select Weaver_ClothReceipt_Code, Vehicle_No from Weaver_Cloth_Receipt_Head where Vehicle_No <> ''", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then

                For i = 0 To Dt1.Rows.Count - 1

                    vVECH_No = Dt1.Rows(i).Item("Vehicle_No").ToString

                    If Trim(vVECH_No) <> "" Then
                        vVECH_No = Common_Procedures.Vehicle_Number_Remove_Unwanted_Spaces(Trim(vVECH_No))

                        cmd.CommandText = "Update Weaver_Cloth_Receipt_Head Set " &
                                            " Vehicle_No = '" & Trim(vVECH_No) & "' " &
                                            " Where Weaver_ClothReceipt_Code = '" & Trim(Dt1.Rows(i).Item("Weaver_ClothReceipt_Code").ToString) & "' "
                        Nr = cmd.ExecuteNonQuery

                    End If

                Next

            End If
            Dt1.Clear()

            tr.Commit()

            cn1.Close()

            MessageBox.Show("Vehicle No. Updated", "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception

            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try

    End Sub

