    Private Sub mnu_Tools_FieldsCheck_EmptyBeamDetails_Updation_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Tools_FieldsCheck_EmptyBeamDetails_Updation.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable

        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDEB123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        cmd.CommandText = "truncate table " & Trim(Common_Procedures.ReportTempTable) & ""
        cmd.ExecuteNonQuery()

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            cmd.CommandText = "truncate table Empty_BeamBagCone_Delivery_Details"
            cmd.ExecuteNonQuery()

            Da = New SqlClient.SqlDataAdapter("select a.* from Empty_BeamBagCone_Delivery_Head a where a.Empty_Beam <> 0 Order by a.Empty_BeamBagCone_Delivery_Date, a.for_orderby, a.Empty_BeamBagCone_Delivery_Code", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then

                For i = 0 To Dt1.Rows.Count - 1

                    cmd.Parameters.Clear()
                    cmd.Parameters.AddWithValue("@DeliveryDate", Dt1.Rows(i).Item("Empty_BeamBagCone_Delivery_Date"))

                    cmd.CommandText = "Insert into Empty_BeamBagCone_Delivery_Details (                            Empty_BeamBagCone_Delivery_Code                 ,                               Company_IdNo                 ,                             Empty_BeamBagCone_Delivery_No                 ,                           for_OrderBy                , Empty_BeamBagCone_Delivery_Date,  Sl_No,                            Empty_Beam                ,                               Beam_Width_IdNo                  ) " &
                                                                    "          Values ('" & Trim(Dt1.Rows(i).Item("Empty_BeamBagCone_Delivery_Code").ToString) & "', " & Str(Val(Dt1.Rows(i).Item("Company_IdNo").ToString)) & ", '" & Trim(Dt1.Rows(i).Item("Empty_BeamBagCone_Delivery_No").ToString) & "', " & Val(Dt1.Rows(i).Item("for_OrderBy").ToString) & ",          @DeliveryDate         ,    1  ,  " & Val(Dt1.Rows(i).Item("Empty_Beam").ToString) & ", " & Str(Val(Dt1.Rows(i).Item("Beam_Width_IdNo").ToString)) & " ) "
                    cmd.ExecuteNonQuery()

                Next
            End If
            Dt1.Clear()

            cmd.CommandText = "truncate table Empty_BeamBagCone_Receipt_Details"
            cmd.ExecuteNonQuery()

            Da = New SqlClient.SqlDataAdapter("select a.* from Empty_BeamBagCone_Receipt_Head a where a.Empty_Beam <> 0 Order by a.Empty_BeamBagCone_Receipt_Date, a.for_orderby, a.Empty_BeamBagCone_Receipt_Code", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then

                For i = 0 To Dt1.Rows.Count - 1

                    cmd.Parameters.Clear()
                    cmd.Parameters.AddWithValue("@ReceiptDate", Dt1.Rows(i).Item("Empty_BeamBagCone_Receipt_Date"))

                    cmd.CommandText = "Insert into Empty_BeamBagCone_Receipt_Details (                            Empty_BeamBagCone_Receipt_Code                 ,                               Company_IdNo                 ,                             Empty_BeamBagCone_Receipt_No                 ,                           for_OrderBy                , Empty_BeamBagCone_Receipt_Date,  Sl_No,                            Empty_Beam                ,                               Beam_Width_IdNo                  ) " &
                                                                    "         Values ('" & Trim(Dt1.Rows(i).Item("Empty_BeamBagCone_Receipt_Code").ToString) & "', " & Str(Val(Dt1.Rows(i).Item("Company_IdNo").ToString)) & ", '" & Trim(Dt1.Rows(i).Item("Empty_BeamBagCone_Receipt_No").ToString) & "', " & Val(Dt1.Rows(i).Item("for_OrderBy").ToString) & ",          @ReceiptDate         ,    1  ,  " & Val(Dt1.Rows(i).Item("Empty_Beam").ToString) & ", " & Str(Val(Dt1.Rows(i).Item("Beam_Width_IdNo").ToString)) & " ) "
                    cmd.ExecuteNonQuery()

                Next
            End If
            Dt1.Clear()

            tr.Commit()

            cn1.Close()

            MessageBox.Show("EmptyBeam Details Updated", "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try

    End Sub

