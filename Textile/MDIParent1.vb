    Private Sub mnu_Tools_FieldsCheck_FieldsCheck_JobWorker_Doffing_Pavu_Consumption_Deletion_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Tools_FieldsCheck_FieldsCheck_JobWorker_Doffing_Pavu_Consumption_Deletion.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim da As New SqlClient.SqlDataAdapter
        Dim da1 As New SqlClient.SqlDataAdapter
        Dim cmd As New SqlClient.SqlCommand
        Dim dt As New DataTable
        Dim dt1 As New DataTable
        Dim flperc As Single = 0
        Dim flmtr As Single = 0
        Dim fmtr As Single = 0
        Dim InvMtrFld As Single = 0
        Dim InvMtrShtFld As Single = 0
        Dim Nr As Integer = 0
        Dim g As New Password

        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLD1234" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED.....", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        Try

            cmd.CommandText = "Delete from Stock_Pavu_Processing_Details where Reference_Code LIKE 'PCDOF-%'"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "Delete from Stock_Yarn_Processing_Details where Reference_Code LIKE 'PCDOF-%'"
            cmd.ExecuteNonQuery()

            MessageBox.Show("Jobworker - Pavu Consumption Meter Successfully Updated", "FOR FIELDS UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            MessageBox.Show(ex.Message, "FOR  FIELDS CHECK...", MessageBoxButtons.OK, MessageBoxIcon.Error)

        Finally
            dt.Dispose()
            da.Dispose()

        End Try

    End Sub

