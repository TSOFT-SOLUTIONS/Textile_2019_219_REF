    Private Sub mnu_FiledCheck_Processing_Receipt_Selection_Pending_Updation_PrakashCottex_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_FiledCheck_Processing_Receipt_Selection_Pending_Updation_PrakashCottex.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim DelvCode As String = ""
        Dim DetailSl_No As Integer = 0
        Dim Pk_Condition As String = "FPRRC-"

        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDCOTTEX123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED.....", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            cmd.CommandText = "Update Textile_Processing_Delivery_Details set Receipt_Meters = 0, Return_Meters = 0, Receipt_Qty = 0 ,Return_Qty =0 ,Receipt_Pcs =0 ,Return_Pcs =0 , Receipt_Weight =0 ,Return_Weight =0"
            cmd.ExecuteNonQuery()

            Da = New SqlClient.SqlDataAdapter("select Cloth_Processing_Delivery_code ,Cloth_Processing_Delivery_Slno ,sum(Receipt_Meters) as Recp_Mtrs ,sum(Receipt_Weight) as Recp_Wgt,sum(Receipt_Pcs) as Recp_Pcs,sum(Receipt_Qty) as Recp_Qty ,sum(ExcSht_Meters) as ExSht_Mtrs  from Textile_Processing_Receipt_Details a Where a.Cloth_Processing_Delivery_code <> '' group by Cloth_Processing_Delivery_code ,Cloth_Processing_Delivery_Slno ", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then
                For i = 0 To Dt1.Rows.Count - 1

                    DelvCode = Trim(Dt1.Rows(i).Item("Cloth_Processing_Delivery_code").ToString)

                    DetailSl_No = Val(Dt1.Rows(i).Item("Cloth_Processing_Delivery_Slno").ToString)

                    cmd.CommandText = "Update Textile_Processing_Delivery_Details set Receipt_Meters = " & Val(Dt1.Rows(i).Item("Recp_Mtrs").ToString) + Val(Dt1.Rows(i).Item("ExSht_Mtrs").ToString) & " Where Cloth_Processing_Delivery_code = '" & Trim(DelvCode) & "' and Cloth_Processing_Delivery_Slno = " & Val(DetailSl_No)
                    cmd.ExecuteNonQuery()

                    cmd.CommandText = "Update Textile_Processing_Delivery_Details set Receipt_Qty = " & Str(Val(Val(Dt1.Rows(i).Item("Recp_Qty").ToString))) & " Where Cloth_Processing_Delivery_code = '" & Trim(DelvCode) & "' and Cloth_Processing_Delivery_Slno = " & Val(DetailSl_No)
                    cmd.ExecuteNonQuery()

                    cmd.CommandText = "Update Textile_Processing_Delivery_Details set Receipt_Pcs = " & Str(Val(Val(Dt1.Rows(i).Item("Recp_Pcs").ToString))) & " Where Cloth_Processing_Delivery_code = '" & Trim(DelvCode) & "'  and Cloth_Processing_Delivery_Slno = " & Val(DetailSl_No)
                    cmd.ExecuteNonQuery()

                    cmd.CommandText = "Update Textile_Processing_Delivery_Details set Receipt_Weight = " & Str(Val(Val(Dt1.Rows(i).Item("Recp_Wgt").ToString))) & " Where Cloth_Processing_Delivery_code = '" & Trim(DelvCode) & "' and Cloth_Processing_Delivery_Slno = " & Val(DetailSl_No)
                    cmd.ExecuteNonQuery()

                Next
            End If
            Dt1.Clear()


            cmd.CommandText = "Update  Textile_Processing_Delivery_Details set Lot_Complete_status = 0 , Lot_Complete_Code  = ''"
            cmd.ExecuteNonQuery()

            Da = New SqlClient.SqlDataAdapter("select  a.Cloth_Processing_Receipt_Code ,a.Cloth_Processing_Delivery_code ,a.Cloth_Processing_Delivery_Slno  ,b.Ledger_IdNo from Textile_Processing_Receipt_Details a LEFT OUTER JOIN Textile_Processing_Receipt_Head b On a.Cloth_Processing_Receipt_Code = b.ClothProcess_Receipt_Code Where a.Cloth_Processing_Delivery_code <> '' and b.Lot_Status = 1 ORDER BY a.Cloth_Processing_Receipt_Date DESC ,a.For_OrderBy DESC ,a.Cloth_Processing_Receipt_Code ", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then
                For i = 0 To Dt1.Rows.Count - 1
                    cmd.CommandText = "Update  Textile_Processing_Delivery_Details set Lot_Complete_status = 1 , Lot_Complete_Code  = '" & Trim(Pk_Condition) & Trim(Dt1.Rows(i).Item("Cloth_Processing_Receipt_Code").ToString) & "' Where Lot_Complete_status = 0 and Cloth_Processing_Delivery_code = '" & Trim(Dt1.Rows(i).Item("Cloth_Processing_Delivery_code").ToString) & "' and Cloth_Processing_Delivery_SlNo = " & Str(Val(Dt1.Rows(i).Item("Cloth_Processing_Delivery_Slno").ToString)) & " and Ledger_IdNo = " & Str(Val(Dt1.Rows(i).Item("Ledger_IdNo").ToString))
                    cmd.ExecuteNonQuery()
                Next
            End If
            Dt1.Clear()



            tr.Commit()

            cn1.Close()

            MessageBox.Show("Successfully Updated..", "PROCESSING DELIVERY AND RECEIPT VALUES UPDATED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try

    End Sub

