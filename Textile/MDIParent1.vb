    Private Sub Set_FnRange(ByVal Sts As String)
        Dim cmd As New SqlClient.SqlCommand
        Dim da1 As SqlClient.SqlDataAdapter
        Dim Cn2 As New SqlClient.SqlConnection
        Dim dt1 As DataTable
        Dim st_year As Integer = 0, en_year As Integer = 0
        Dim st_year_CUR As Integer = 0, en_year_CUR As Integer = 0
        Dim amt As Double = 0
        Dim dttm As DateTime
        Dim CmpGrp_Fromdate As DateTime
        Dim CmpGrp_Todate As DateTime
        Dim con As New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        Dim CMP_frmdate As Integer
        Dim CMP_todate As Integer

        CMP_frmdate = Val(Microsoft.VisualBasic.Right(Common_Procedures.Company_FromDate, 4))
        CMP_todate = Val(Microsoft.VisualBasic.Right(Common_Procedures.Company_ToDate, 4))

        If Trim(UCase(Sts)) = "PREVIOUS" Then
            st_year = CMP_frmdate - 1
            en_year = CMP_todate - 1

            If st_year = en_year Then
                en_year = en_year + 1
            ElseIf st_year > en_year Then
                Exit Sub
            End If

        ElseIf Trim(UCase(Sts)) = "NEXT" Then
            st_year = CMP_frmdate + 1
            en_year = CMP_todate + 1

            If st_year = en_year Then
                st_year = st_year - 1
            ElseIf st_year > en_year Then
                Exit Sub
            End If

        End If



        Cn2 = New SqlClient.SqlConnection(Common_Procedures.ConnectionString_CompanyGroupdetails)

        da1 = New SqlClient.SqlDataAdapter("select * from " & Trim(Common_Procedures.CompanyDetailsDataBaseName) & "..CompanyGroup_Head where CompanyGroup_IdNo = " & Str(Val(Common_Procedures.CompGroupIdNo)), Cn2)
        dt1 = New DataTable
        da1.Fill(dt1)
        If dt1.Rows.Count > 0 Then
            If IsDBNull(dt1.Rows(0).Item("From_Date").ToString) = False Then
                If IsDate(dt1.Rows(0).Item("From_Date").ToString) = True Then
                    CmpGrp_Fromdate = dt1.Rows(0).Item("From_Date")
                End If
            End If
            If IsDBNull(dt1.Rows(0).Item("To_Date").ToString) = False Then
                If IsDate(dt1.Rows(0).Item("To_Date").ToString) = True Then
                    CmpGrp_Todate = dt1.Rows(0).Item("To_Date")
                End If
            End If
        End If
        dt1.Clear()

        st_year_CUR = Val(Microsoft.VisualBasic.Right(CmpGrp_Fromdate, 4))
        en_year_CUR = Val(Microsoft.VisualBasic.Right(CmpGrp_Todate, 4))

        btn_Prev_Year.Enabled = True
        If st_year < st_year_CUR Then
            ' btn_Prev_Year.Enabled = False
            Exit Sub
        End If

        btn_Next_Year.Enabled = True
        If en_year > en_year_CUR Then
            'btn_Next_Year.Enabled = False
            Exit Sub
        End If

        btn_Cur_Year.Text = Trim(st_year) & "-" & Trim(en_year)

        Common_Procedures.FnYearCode = Trim(Microsoft.VisualBasic.Right(Val(st_year), 2)) & "-" & Trim(Microsoft.VisualBasic.Right(Val(en_year), 2))

        dttm = New DateTime(Val(st_year), Microsoft.VisualBasic.DateAndTime.Month(CmpGrp_Fromdate), Microsoft.VisualBasic.DateAndTime.Day(CmpGrp_Fromdate))
        Common_Procedures.Company_FromDate = dttm

        dttm = New DateTime(Val(en_year), Microsoft.VisualBasic.DateAndTime.Month(CmpGrp_Todate), Microsoft.VisualBasic.DateAndTime.Day(CmpGrp_Todate))
        Common_Procedures.Company_ToDate = dttm

        Me.Text = Common_Procedures.CompGroupIdNo & ". " & Common_Procedures.CompGroupName & " (" & st_year_CUR & "-" & en_year_CUR & ")         -          " & Common_Procedures.Company_FromDate & "  TO  " & Common_Procedures.Company_ToDate

        Call Common_Procedures.AccountsVoucher_Posting_For_ProfitAndLoss()

        mnu_Opening_Main.Enabled = False
        If Year(Common_Procedures.Company_FromDate) = Val(Microsoft.VisualBasic.Left(Common_Procedures.FnRange, 4)) Then
            mnu_Opening_Main.Enabled = True
        End If


        ' Close all child forms of the parent.
        For Each ChildForm As Form In Me.MdiChildren
            ChildForm.Close()
            'If Not (Me.ActiveMdiChild Is Nothing) Then
            '    Dim f As Interface_MDIActions = ChildForm
            '    Try
            '        f.new_record()
            '    Catch ex As Exception
            '        ChildForm.Close()
            '    End Try
            'End If
        Next

        Show_Hide_VAT_Entry_Menus()
        MdiMenu_VisiblitySettings(mnu_Entry_Textile_Main)

    End Sub

