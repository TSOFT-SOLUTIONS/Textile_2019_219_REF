    Private Sub mnu_Tools_Fields_Check_FP_Item_Transfer_Updation_Jeno_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Tools_Fields_Check_FP_Item_Transfer_Updation_Jeno.Click
        Dim tr As SqlClient.SqlTransaction
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim Da1 As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim inpno As String = ""
        Dim vNew_Code As String = "", vNew_No As String = ""
        Dim vComp_IdNo As Integer = 0
        Dim vProcItmFrm_IdNo As Integer = 0, vProcItmTo_IdNo As Integer = 0
        Dim vSNo As Integer = 0
        Dim vQty As Single = 0, vMtrs As Single = 0, vOrdBy_No As Single = 0
        Dim I As Integer = 0
        Dim vRacfrm_IdNo As Integer = 0, vRacTo_IdNo As Integer = 0
        Dim Pk_Condition As String = "FPITM-"
        Dim Partcls As String = ""
        Dim PBlNo As String = ""
        Dim EntID As String = ""


        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "TSJ1234" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED....", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        inpno = InputBox("Enter Rack Id.No. to Transfer", "FOR AUTO ITEM TRANSFER...")
        If Val(inpno) = 0 Then Exit Sub

        Me.Cursor = Cursors.WaitCursor

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        tr = cn1.BeginTransaction

        Try

            cmd.Connection = cn1
            cmd.Transaction = tr

            cmd.Parameters.Clear()
            cmd.Parameters.AddWithValue("@Entry_Date", Date.Today.Date)


            cmd.CommandText = "Truncate table " & Trim(Common_Procedures.ReportTempSubTable) & ""
            cmd.ExecuteNonQuery()

            cmd.CommandText = "Insert into " & Trim(Common_Procedures.ReportTempSubTable) & " (    Int1               ,    name1              ,    INT2     ,    name2  ,       Int3     ,        meters1  ) " &
                                            " Select       tP.Processed_Item_IdNo, tP.Processed_Item_Name, tE.Rack_IdNo, tE.Rack_No, sum(a.Quantity),  sum(a.Meters)  from Stock_Item_Processing_Details a INNER JOIN Company_Head tZ ON a.Company_IdNo <> 0 and a.Company_IdNo = tZ.Company_IdNo INNER JOIN Processed_Item_Head tP ON a.Item_IdNo <> 0 and a.Item_IdNo = tP.Processed_Item_IdNo INNER JOIN Rack_Head tE ON a.Rack_IdNo <> 0 and a.Rack_IdNo = tE.Rack_IdNo Where a.DeliveryTo_StockIdNo = " & Str(Val(Common_Procedures.CommonLedger.Godown_Ac)) & "  and a.Rack_Idno <> 0 and a.Quantity <> 0 group by tP.Processed_Item_IdNo, tP.Processed_Item_Name, tE.Rack_IdNo, tE.Rack_No"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "Insert into " & Trim(Common_Procedures.ReportTempSubTable) & "(  Int1               ,    name1              ,    INT2     ,    name2  ,          Int3     ,           meters1  ) " &
                                             " Select   tP.Processed_Item_IdNo, tP.Processed_Item_Name, tE.Rack_IdNo, tE.Rack_No, -1*sum(a.Quantity),  -1*sum(a.Meters)  from Stock_Item_Processing_Details a INNER JOIN Company_Head tZ ON a.Company_IdNo <> 0 and a.Company_IdNo = tZ.Company_IdNo INNER JOIN Processed_Item_Head tP ON a.Item_IdNo <> 0 and a.Item_IdNo = tP.Processed_Item_IdNo INNER JOIN Rack_Head tE ON a.Rack_IdNo <> 0 and a.Rack_IdNo = tE.Rack_IdNo Where a.ReceivedFrom_StockIdNo = " & Str(Val(Common_Procedures.CommonLedger.Godown_Ac)) & " and a.Rack_Idno <> 0 and a.Quantity <> 0 group by tP.Processed_Item_IdNo, tP.Processed_Item_Name, tE.Rack_IdNo, tE.Rack_No"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "Truncate table " & Trim(Common_Procedures.ReportTempTable) & ""
            cmd.ExecuteNonQuery()

            cmd.CommandText = "Insert into " & Trim(Common_Procedures.ReportTempTable) & " ( Int1, name1, Int2, name2, Int3, meters1 ) Select Int1, name1, Int2, name2, sum(Int3), sum(meters1) from " & Trim(Common_Procedures.ReportTempSubTable) & " group by Int1, name1, Int2, name2 having sum(Int3) <> 0 "
            cmd.ExecuteNonQuery()

            Da1 = New SqlClient.SqlDataAdapter("select Int1 as FinishedProduct_From_IdNo, name1 as FinishedProduct_From_Name, Int2 as Rack_From_IdNo, name2 as Rack_From_Name, Int3 as StkQuantity, meters1 as StkMeters from " & Trim(Common_Procedures.ReportTempTable) & " Where Int2 <> " & Str(Val(inpno)) & " and Int3 <> 0 order by name1, name2", cn1)
            Da1.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da1.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then

                For I = 0 To Dt1.Rows.Count - 1

                    vComp_IdNo = 1

                    vNew_No = Common_Procedures.get_MaxCode(cn1, "Item_Transfer_Head", "Item_Transfer_Code", "For_OrderBy", "", Val(vComp_IdNo), Common_Procedures.FnYearCode, tr)
                    vNew_Code = Trim(Val(vComp_IdNo)) & "-" & Trim(vNew_No) & "/" & Trim(Common_Procedures.FnYearCode)
                    vOrdBy_No = Format(Val(Common_Procedures.OrderBy_CodeToValue(vNew_No)), "########0.00")
                    vProcItmFrm_IdNo = Val(Dt1.Rows(I).Item("FinishedProduct_From_IdNo").ToString)
                    vProcItmTo_IdNo = vProcItmFrm_IdNo
                    vRacfrm_IdNo = Val(Dt1.Rows(I).Item("Rack_From_IdNo").ToString)
                    vRacTo_IdNo = Val(inpno)
                    vQty = Val(Dt1.Rows(I).Item("StkQuantity").ToString)
                    vMtrs = Val(Dt1.Rows(I).Item("StkMeters").ToString)

                    cmd.CommandText = "Insert into Item_Transfer_Head ( Item_Transfer_Code      ,          Company_IdNo       ,       Item_Transfer_No ,                   for_OrderBy                       , Item_Transfer_Date,          Item_From_Idno      ,            Item_To_Idno          ,           RackFrom_IdNo       ,             RackTo_IdNo      ,      Quantity    ,       Meters       ) " &
                                        "          Values             ('" & Trim(vNew_Code) & "', " & Str(Val(vComp_IdNo)) & ", '" & Trim(vNew_No) & "', " & Str(Format(Val(vOrdBy_No), "#########0.00")) & ",     @Entry_Date   , " & Val(vProcItmFrm_IdNo) & ", " & Str(Val(vProcItmTo_IdNo)) & ", " & Str(Val(vRacfrm_IdNo)) & ", " & Str(Val(vRacTo_IdNo)) & ", " & Val(vQty) & ", " & Val(vMtrs) & " ) "
                    cmd.ExecuteNonQuery()

                    Partcls = "Transfer : Ref.No. " & Trim(vNew_No)
                    PBlNo = Trim(vNew_No)
                    EntID = Trim(Pk_Condition) & Trim(vNew_No)


                    cmd.CommandText = "Delete from Stock_Item_Processing_Details Where Company_IdNo = " & Str(Val(vComp_IdNo)) & " and Reference_Code = '" & Trim(Pk_Condition) & Trim(vNew_Code) & "'"
                    cmd.ExecuteNonQuery()

                    cmd.CommandText = "Insert into Stock_Item_Processing_Details ( Reference_Code                      ,            Company_IdNo     ,         Reference_No   ,               For_OrderBy                          ,  Reference_Date,  DeliveryTo_StockIdNo,                                ReceivedFrom_StockIdNo      , Delivery_PartyIdNo, Received_PartyIdNo,         Entry_ID     ,    Party_Bill_No     ,      Particulars       , SL_No,               Item_IdNo           ,           Rack_IdNo           ,         Quantity      ,          Meters    ) " &
                                                               " Values ('" & Trim(Pk_Condition) & Trim(vNew_Code) & "', " & Str(Val(vComp_IdNo)) & ", '" & Trim(vNew_No) & "', " & Str(Format(Val(vOrdBy_No), "########0.00")) & ",    @Entry_Date ,               0      ,  " & Str(Val(Common_Procedures.CommonLedger.Godown_Ac)) & ",              0    ,         0         , '" & Trim(EntID) & "', '" & Trim(PBlNo) & "', '" & Trim(Partcls) & "',  -1  , " & Str(Val(vProcItmFrm_IdNo)) & ", " & Str(Val(vRacfrm_IdNo)) & ", " & Str(Val(vQty)) & ", " & Val(vMtrs) & " ) "
                    cmd.ExecuteNonQuery()

                    cmd.CommandText = "Insert into Stock_Item_Processing_Details ( Reference_Code                      ,            Company_IdNo     ,         Reference_No   ,               For_OrderBy                          ,  Reference_Date,                               DeliveryTo_StockIdNo        , ReceivedFrom_StockIdNo, Delivery_PartyIdNo, Received_PartyIdNo,         Entry_ID     ,    Party_Bill_No     ,      Particulars       , SL_No,               Item_IdNo          ,           Rack_IdNo          ,         Quantity      ,          Meters    ) " &
                                                               " Values ('" & Trim(Pk_Condition) & Trim(vNew_Code) & "', " & Str(Val(vComp_IdNo)) & ", '" & Trim(vNew_No) & "', " & Str(Format(Val(vOrdBy_No), "########0.00")) & ",    @Entry_Date , " & Str(Val(Common_Procedures.CommonLedger.Godown_Ac)) & ",              0        ,              0    ,         0         , '" & Trim(EntID) & "', '" & Trim(PBlNo) & "', '" & Trim(Partcls) & "',   1  , " & Str(Val(vProcItmTo_IdNo)) & ", " & Str(Val(vRacTo_IdNo)) & ", " & Str(Val(vQty)) & ", " & Val(vMtrs) & " ) "
                    cmd.ExecuteNonQuery()


                Next I

            End If
            Dt1.Clear()

            tr.Commit()

            Me.Cursor = Cursors.Default

            MessageBox.Show("Transfered Successfully!!!", "FOR AUTO ITEM TRANSFER...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            Me.Cursor = Cursors.Default
            MessageBox.Show(ex.Message, "FOR AUTO ITEM TRANSFER...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            cmd.Dispose()

            cn1.Close()
            cn1.Dispose()



            tr.Dispose()
            Da1.Dispose()
            Dt1.Dispose()
            Me.Cursor = Cursors.Default

        End Try

    End Sub

    Private Sub mnu_Tools_Fields_Check_Employee_Ledger_Type_Updation_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Tools_Fields_Check_Employee_Ledger_Type_Updation.Click
