    Private Sub mnu_Tools_FieldsCheck_Asia_Beam_ProductionMeters_Updation_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Tools_FieldsCheck_Asia_Beam_ProductionMeters_Updation.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim RecCode As String = ""
        Dim Clo_ID As Integer = 0, Lm_ID As Integer = 0
        Dim Wdth_Typ As String = ""
        Dim RecMtrs As Single = 0
        Dim ChkMtrs As Single = 0
        Dim ConsPavu_Rec As Single = 0
        Dim BeamConPavu_Rec As Single = 0
        Dim BeamCons_Pavu As Single = 0
        Dim ConsPavu_Chk As Single = 0
        Dim BeamConPavu_Chk As Single = 0

        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDASIA123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If



        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        cmd.CommandText = "truncate table " & Trim(Common_Procedures.ReportTempTable) & ""
        cmd.ExecuteNonQuery()

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            cmd.CommandText = "Update Weaver_Cloth_Receipt_Head set BeamConsumption_Receipt = 0, BeamConsumption_Checking = 0, BeamConsumption_Meters = 0"
            cmd.ExecuteNonQuery()

            Da = New SqlClient.SqlDataAdapter("select * from Weaver_Cloth_Receipt_Head where set_code1 <> '' and Beam_No1 <> ''", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then
                For i = 0 To Dt1.Rows.Count - 1

                    RecCode = Dt1.Rows(i).Item("Weaver_ClothReceipt_Code").ToString

                    Clo_ID = Val(Dt1.Rows(i).Item("Cloth_IdNo").ToString)
                    Lm_ID = Val(Dt1.Rows(i).Item("Loom_IdNo").ToString)

                    Wdth_Typ = Dt1.Rows(i).Item("Width_Type").ToString

                    RecMtrs = Val(Dt1.Rows(i).Item("Receipt_Meters").ToString)

                    ChkMtrs = Val(Dt1.Rows(i).Item("Type1_Checking_Meters").ToString) + Val(Dt1.Rows(i).Item("Type2_Checking_Meters").ToString) + Val(Dt1.Rows(i).Item("Type3_Checking_Meters").ToString) + Val(Dt1.Rows(i).Item("Type4_Checking_Meters").ToString) + Val(Dt1.Rows(i).Item("Type5_Checking_Meters").ToString)

                    ConsPavu_Rec = 0
                    BeamConPavu_Rec = 0
                    ConsumedPavu_Calculation(cn1, RecCode, Clo_ID, Lm_ID, Val(RecMtrs), Trim(Wdth_Typ), ConsPavu_Rec, BeamConPavu_Rec, tr)

                    ConsPavu_Chk = 0
                    BeamConPavu_Chk = 0
                    ConsumedPavu_Calculation(cn1, RecCode, Clo_ID, Lm_ID, Val(ChkMtrs), Trim(Wdth_Typ), ConsPavu_Chk, BeamConPavu_Chk, tr)

                    BeamCons_Pavu = 0
                    If BeamConPavu_Chk <> 0 Then
                        BeamCons_Pavu = BeamConPavu_Chk
                    Else
                        BeamCons_Pavu = BeamConPavu_Rec
                    End If

                    cmd.CommandText = "Update Weaver_Cloth_Receipt_Head set BeamConsumption_Receipt = " & Str(Val(BeamConPavu_Rec)) & ", BeamConsumption_Checking = " & Str(Val(BeamConPavu_Chk)) & ", BeamConsumption_Meters = " & Str(Val(BeamCons_Pavu)) & " Where Weaver_ClothReceipt_Code = '" & Trim(RecCode) & "'"
                    cmd.ExecuteNonQuery()

                    If Trim(Dt1.Rows(i).Item("set_code1").ToString) <> "" And Trim(Dt1.Rows(i).Item("Beam_No1").ToString) <> "" Then
                        cmd.CommandText = "insert into " & Trim(Common_Procedures.ReportTempTable) & "(name1, name2, meters1) values ('" & Trim(Dt1.Rows(i).Item("set_code1").ToString) & "', '" & Trim(Dt1.Rows(i).Item("Beam_No1").ToString) & "', " & Str(Val(BeamCons_Pavu)) & ")"
                        cmd.ExecuteNonQuery()
                    End If

                    If Trim(Dt1.Rows(i).Item("set_code2").ToString) <> "" And Trim(Dt1.Rows(i).Item("Beam_No2").ToString) <> "" Then
                        cmd.CommandText = "insert into " & Trim(Common_Procedures.ReportTempTable) & "(name1, name2, meters1) values ('" & Trim(Dt1.Rows(i).Item("set_code2").ToString) & "', '" & Trim(Dt1.Rows(i).Item("Beam_No2").ToString) & "', " & Str(Val(BeamCons_Pavu)) & ")"
                        cmd.ExecuteNonQuery()
                    End If


                Next
            End If
            Dt1.Clear()

            cmd.CommandText = "Update Stock_SizedPavu_Processing_Details set Production_Meters = 0"
            cmd.ExecuteNonQuery()

            Da = New SqlClient.SqlDataAdapter("select name1, name2, sum(meters1) as BeamConsMeters from " & Trim(Common_Procedures.ReportTempTable) & " group by name1, name2 order by name1, name2 ", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then
                For i = 0 To Dt1.Rows.Count - 1

                    cmd.CommandText = "Update Stock_SizedPavu_Processing_Details set Production_Meters = " & Str(Val(Dt1.Rows(i).Item("BeamConsMeters").ToString)) & " Where Set_Code = '" & Trim(Dt1.Rows(i).Item("Name1").ToString) & "' and Beam_No = '" & Trim(Dt1.Rows(i).Item("Name2").ToString) & "'"
                    cmd.ExecuteNonQuery()

                Next
            End If
            Dt1.Clear()


            '--- Knotting Production Meter Updation

            cmd.CommandText = "Update Beam_Knotting_Head set Production_Meters = 0"
            cmd.ExecuteNonQuery()

            Da = New SqlClient.SqlDataAdapter("select Beam_Knotting_Code, sum(ReceiptMeters_Receipt) as RecMeters from Weaver_Cloth_Receipt_Head where Beam_Knotting_Code <> '' group by Beam_Knotting_Code having sum(ReceiptMeters_Receipt) <> 0 order by Beam_Knotting_Code", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then
                For i = 0 To Dt1.Rows.Count - 1

                    cmd.CommandText = "Update Beam_Knotting_Head set Production_Meters = " & Str(Val(Dt1.Rows(i).Item("RecMeters").ToString)) & " Where Beam_Knotting_Code = '" & Trim(Dt1.Rows(i).Item("Beam_Knotting_Code").ToString) & "'"
                    cmd.ExecuteNonQuery()

                Next
            End If
            Dt1.Clear()



            tr.Commit()

            cn1.Close()

            MessageBox.Show("Fields Verified", "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try

    End Sub

