    Private Sub mnu_Tools_FieldsCheck_Delete_Jobworker_Production_Stock_Posting_Click(sender As Object, e As EventArgs) Handles mnu_Tools_FieldsCheck_Delete_Jobworker_Production_Stock_Posting.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim Nr As Long = 0
        Dim RcptWgt As String = 0
        Dim RcptBg As String = 0
        Dim RcptCn As String = 0


        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLD123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            If Common_Procedures.settings.JobWorker_Pavu_Yarn_Stock_Posting_IN_Production = 0 Then

                cmd.CommandText = "Delete from Stock_Pavu_Processing_Details Where Reference_Code LIKE 'JPROD-%'"
                Nr = cmd.ExecuteNonQuery()
                cmd.CommandText = "Delete from Stock_Yarn_Processing_Details Where Reference_Code LIKE 'JPROD-%'"
                Nr = cmd.ExecuteNonQuery()

            End If

            tr.Commit()

            cn1.Close()

            MessageBox.Show("Jobworker Production Stock Posting Deleted", "FOR FIEDS CHECK...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR FIELDS CHECK...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try

    End Sub

