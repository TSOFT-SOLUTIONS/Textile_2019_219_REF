    Private Sub MDIParent1_Activated(ByVal sender As Object, ByVal e As System.EventArgs) Handles Me.Activated
        Dim vToYr As Integer = 0

        If Common_Procedures.ENTRY_SAVING_MESSAGE_STATUS = True Then Exit Sub
        mnu_Opening_Main.Enabled = True
        mnu_Opening_Sizing_Software_Main.Enabled = True
        mnu_Opening_OE_Software_Main.Enabled = True
        If Trim(UCase(Common_Procedures.settings.CustomerCode)) <> "1035" Then                   '---- Kalaimagal Textiles (Avinashi)
            mnu_Opening_Main.Enabled = False
            mnu_Opening_Sizing_Software_Main.Enabled = False
            mnu_Opening_OE_Software_Main.Enabled = False
            If Year(Common_Procedures.Company_FromDate) = Val(Microsoft.VisualBasic.Left(Common_Procedures.FnRange, 4)) Then
                mnu_Opening_Main.Enabled = True
                mnu_Opening_Sizing_Software_Main.Enabled = True
                mnu_Opening_OE_Software_Main.Enabled = True
            End If
        End If

        If FrmLdSTS = True Then
            Show_Hide_VAT_Entry_Menus()
            MdiMenu_VisiblitySettings(mnu_Entry_Textile_Main)
        End If

        vToYr = Val(Microsoft.VisualBasic.Right(Common_Procedures.CompGroupFnRange, 4))

        mnu_Company_Create_Next_Year.Text = "D. Create Next Year   (" & Trim(vToYr) & "-" & Trim(vToYr + 1) & ")"

        lbl_UserName.Text = "USER : " & Trim(UCase(Common_Procedures.User.Name))

        Me.Cursor = Cursors.Default
        FrmLdSTS = False

    End Sub

    Private Sub MDIParent1_GotFocus(ByVal sender As Object, ByVal e As System.EventArgs) Handles Me.GotFocus
        If Common_Procedures.ENTRY_SAVING_MESSAGE_STATUS = True Then Exit Sub
        mnu_Opening_Main.Enabled = True
        If Trim(UCase(Common_Procedures.settings.CustomerCode)) <> "1035" Then                   '---- Kalaimagal Textiles (Avinashi)
            mnu_Opening_Main.Enabled = False
            If Year(Common_Procedures.Company_FromDate) = Val(Microsoft.VisualBasic.Left(Common_Procedures.FnRange, 4)) Then
                mnu_Opening_Main.Enabled = True
            End If
        End If
        If Me.ActiveMdiChild Is Nothing Then
            Menu_Selection()
        End If

        If Trim(UCase(Common_Procedures.settings.CustomerCode)) = "1307" Then '---- Sri Sugam Textile (Karumanthampatti)

            If Me.ActiveMdiChild Is Nothing Then

                Try

                    If Common_Procedures.SerialPort1.IsOpen Then
                        Common_Procedures.SerialPort1.Close()
                        'Common_Procedures.SerialPort1.Dispose()


                        'MessageBox.Show("PORT CLOSED (COM" + DefaultPortNo.ToString & ")")
                        ' lbl_PortConnection.Text = "Port Closed"
                        ' txt_Weight_FromMachine.Text = Nothing

                        Threading.Thread.Sleep(5000)

                    End If

                Catch ex As Exception
                    '----

                End Try

            End If

        End If



    End Sub

