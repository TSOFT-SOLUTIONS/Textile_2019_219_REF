    Private Sub Create_NewYear_Alert()
        Dim Cn1 As New SqlClient.SqlConnection
        Dim da1 As SqlClient.SqlDataAdapter
        Dim dt1 As DataTable
        Dim STS As Integer = 0
        Dim vChngYrSTSSTS As Boolean = True
        Dim vToYr As Integer = 0
        Dim dttm1 As DateTime
        Dim dttm2 As DateTime
        Dim vAlertSTS As Boolean = False
        Dim vCompGrpSurNm_Selc As String = ""
        Dim vCompGrpSurNm_DB As String = ""

        vToYr = Val(Microsoft.VisualBasic.Right(Common_Procedures.CompGroupFnRange, 4))

        vAlertSTS = False
        If vToYr = Year(Date.Today) And (Month(Date.Today) = 3 Or Month(Date.Today) = 4) Then

            dttm1 = New DateTime(Year(Date.Today), 3, 28)
            dttm2 = New DateTime(Year(Date.Today), 4, 14)

            If DateDiff("d", dttm1.ToShortDateString, Date.Today.ToShortDateString) > 0 Then

                If DateDiff("d", Date.Today.ToShortDateString, dttm2.ToShortDateString) > 0 Then

                    Cn1 = New SqlClient.SqlConnection(Common_Procedures.ConnectionString_CompanyGroupdetails)
                    Cn1.Open()

                    vChngYrSTSSTS = Common_Procedures.is_Separate_New_CompanyGroup_Created_for_NextYear(Cn1, Common_Procedures.CompGroupIdNo, Common_Procedures.CompGroupName)

                    'vCompGrpSurNm_Selc = Common_Procedures.Remove_NonCharacters(Common_Procedures.CompGroupName)

                    'vChngYrSTSSTS = True

                    'STS = 0
                    'da1 = New SqlClient.SqlDataAdapter("select * from CompanyGroup_Head Where CompanyGroup_IdNo <> " & Str(Val(Common_Procedures.CompGroupIdNo)), Cn1)
                    'dt1 = New DataTable
                    'da1.Fill(dt1)
                    'If dt1.Rows.Count > 0 Then

                    '    For i = 0 To dt1.Rows.Count - 1
                    '        vCompGrpSurNm_DB = ""
                    '        If IsDBNull(dt1.Rows(i).Item("CompanyGroup_Name").ToString) = False Then
                    '            vCompGrpSurNm_DB = Common_Procedures.Remove_NonCharacters(dt1.Rows(i).Item("CompanyGroup_Name").ToString)
                    '        End If

                    '        If Trim(UCase(vCompGrpSurNm_DB)) = Trim(UCase(vCompGrpSurNm_Selc)) Then

                    '            If IsDBNull(dt1.Rows(i).Item("Financial_Range").ToString) = False Then
                    '                If Val(Year(Now) + 1) = Val(Microsoft.VisualBasic.Right(Trim(dt1.Rows(0).Item("Financial_Range").ToString), 4)) Then
                    '                    vChngYrSTSSTS = False
                    '                    Exit For
                    '                End If
                    '            End If
                    '        End If

                    '    Next i

                    'End If
                    'dt1.Clear()
                    'dt1.Dispose()
                    'da1.Dispose()

                    Cn1.Close()

                    If vChngYrSTSSTS = True Then

                        Cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
                        Cn1.Open()

                        STS = 0
                        da1 = New SqlClient.SqlDataAdapter("select * from Settings_Head", Cn1)
                        dt1 = New DataTable
                        da1.Fill(dt1)
                        If dt1.Rows.Count > 0 Then
                            If IsDBNull(dt1.Rows(0).Item("ChangePeriod_CreateNewYear_Alert_Dont_Ask_Status").ToString) = False Then
                                STS = Val(dt1.Rows(0).Item("ChangePeriod_CreateNewYear_Alert_Dont_Ask_Status").ToString)
                            End If
                        End If
                        dt1.Clear()
                        dt1.Dispose()
                        da1.Dispose()
                        Cn1.Close()

                        If STS = 0 Then
                            Dim f As New Change_Period("AUTO")
                            f.ShowDialog()
                        End If

                    End If
                    Cn1.Dispose()

                End If

            End If

        Else

            Dim Cn2 As New SqlClient.SqlConnection
            Dim cmd As New SqlClient.SqlCommand
            Dim Nr As Long = 0

            Cn2 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
            Cn2.Open()

            cmd.Connection = Cn2

            Nr = 0
            cmd.CommandText = "update settings_head set ChangePeriod_CreateNewYear_Alert_Dont_Ask_Status = 0"
            Nr = cmd.ExecuteNonQuery()
            If Nr = 0 Then
                cmd.CommandText = "Insert into settings_head (ChangePeriod_CreateNewYear_Alert_Dont_Ask_Status) values (0)"
                cmd.ExecuteNonQuery()
            End If

            cmd.Dispose()

            Cn2.Close()
            Cn2.Dispose()

        End If

    End Sub

