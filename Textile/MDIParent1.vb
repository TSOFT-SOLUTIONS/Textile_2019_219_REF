                    If IsDBNull(dt1.Rows(0).Item("To_Date").ToString) = False Then
                        If IsDate(dt1.Rows(0).Item("To_Date").ToString) = True Then
                            CmpGrp_Todate = dt1.Rows(0).Item("To_Date")
                        End If
                    End If
                End If
                dt1.Clear()


                dttm = New DateTime(Val(Microsoft.VisualBasic.Right(Common_Procedures.FnRange, 4)) - 1, Microsoft.VisualBasic.DateAndTime.Month(CmpGrp_Fromdate), Microsoft.VisualBasic.DateAndTime.Day(CmpGrp_Fromdate))
                Common_Procedures.Company_FromDate = dttm

                dttm = New DateTime(Val(Microsoft.VisualBasic.Right(Common_Procedures.FnRange, 4)), Microsoft.VisualBasic.DateAndTime.Month(CmpGrp_Todate), Microsoft.VisualBasic.DateAndTime.Day(CmpGrp_Todate))
                Common_Procedures.Company_ToDate = dttm

