    Private Sub mnu_tools_Field_Check_Invoice_Folding_Meter_Updation_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_tools_Field_Check_Invoice_Folding_Meter_Updation.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim da As New SqlClient.SqlDataAdapter
        Dim da1 As New SqlClient.SqlDataAdapter
        Dim cmd As New SqlClient.SqlCommand
        Dim dt As New DataTable
        Dim dt1 As New DataTable
        Dim flperc As Single = 0
        Dim flmtr As Single = 0
        Dim fmtr As Single = 0
        Dim InvMtrFld As Single = 0
        Dim InvMtrShtFld As Single = 0
        'Dim Fold_Meter As Single = 0
        'Dim Fold_Short_Meter As Single = 0
        Dim Nr As Integer = 0
        Dim g As New Password



        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDINV123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED.....", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1


        Try

            da = New SqlClient.SqlDataAdapter("select * from ClothSales_Invoice_Details", cn1)
            dt = New DataTable
            da.Fill(dt)
            If dt.Rows.Count > 0 Then
                For i = 0 To dt.Rows.Count - 1

                    InvMtrFld = 0 : InvMtrShtFld = 0
                    If Trim(UCase(Common_Procedures.settings.CustomerCode)) = "1044" Then
                        InvMtrFld = Val(dt.Rows(i).Item("Meters").ToString) * 100 / Val(dt.Rows(i).Item("Fold_Perc").ToString)
                        InvMtrShtFld = Val(dt.Rows(i).Item("Short_Meters").ToString) * 100 / Val(dt.Rows(i).Item("Fold_Perc").ToString)

                    Else

                        flperc = 100 - Val(dt.Rows(i).Item("Fold_Perc").ToString)

                        flmtr = Format(Val(dt.Rows(i).Item("Meters").ToString) * flperc / 100, "#########0.00")
                        flmtr = Math.Abs(Val(flmtr))
                        If Trim(UCase(Common_Procedures.settings.CustomerCode)) <> "1035" And Trim(UCase(Common_Procedures.settings.CustomerCode)) <> "1152" Then
                            flmtr = Common_Procedures.Meter_RoundOff(flmtr)
                        End If
                        If Val(dt.Rows(i).Item("Fold_Perc").ToString) > 100 Then
                            InvMtrFld = Val(dt.Rows(i).Item("Meters").ToString) + flmtr
                        Else
                            InvMtrFld = Val(dt.Rows(i).Item("Meters").ToString) - flmtr
                        End If


                        If Val(dt.Rows(i).Item("Short_Meters").ToString) <> 0 Then
                            flmtr = Format(Val(dt.Rows(i).Item("Short_Meters").ToString) * flperc / 100, "#########0.00")
                            flmtr = Math.Abs(Val(flmtr))
                            If Trim(UCase(Common_Procedures.settings.CustomerCode)) <> "1035" And Trim(UCase(Common_Procedures.settings.CustomerCode)) <> "1152" Then
                                flmtr = Common_Procedures.Meter_RoundOff(flmtr)
                            End If
                            If Val(dt.Rows(i).Item("Fold_Perc").ToString) > 100 Then
                                InvMtrShtFld = Val(dt.Rows(i).Item("Short_Meters").ToString) - flmtr
                            Else
                                InvMtrShtFld = Val(dt.Rows(i).Item("Short_Meters").ToString) + flmtr
                            End If
                        End If

                    End If

                    cmd.CommandText = "Update ClothSales_Invoice_Details set Fold_Meter = " & Str(Val(InvMtrFld)) & ", Fold_Short_Meter = " & Str(Val(InvMtrShtFld)) & " Where ClothSales_Invoice_Code  = '" & Trim(dt.Rows(i).Item("ClothSales_Invoice_Code").ToString) & "' and ClothSales_Invoice_SlNo =  " & Str(Val(dt.Rows(i).Item("ClothSales_Invoice_SlNo").ToString))
                    cmd.ExecuteNonQuery()

                Next

            End If
            dt.Clear()
            da.Dispose()
            cn1.Close()

            MessageBox.Show("Invoice Folding Meter Successfully Updated", "FOR FIELDS UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            MessageBox.Show(ex.Message, "FOR  MOVING...", MessageBoxButtons.OK, MessageBoxIcon.Error)

        Finally
            dt.Dispose()
            da.Dispose()

        End Try
    End Sub

