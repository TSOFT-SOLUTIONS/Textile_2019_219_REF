    Private Sub mnu_Tools_FieldsCheck_BRT_Wages_Code_Updation_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Tools_FieldsCheck_BRT_Wages_Code_Updation.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim Sno As Integer = 0

        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLD123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            cmd.CommandText = "Update Weaver_Cloth_Receipt_Head Set Weaver_IR_Wages_Code = '', Weaver_Wages_Code = '', Weaver_Wages_Increment = 0 Where Weaver_ClothReceipt_Code LIKE '%/20-21'"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "Update Weaver_Cloth_Receipt_Head Set Weaver_IR_Wages_Code = b.Weaver_Wages_Code From Weaver_Cloth_Receipt_Head a, Weaver_Wages_Head b Where a.Weaver_ClothReceipt_Code LIKE '%/20-21' and b.Weaver_Wages_Code LIKE 'GWWAG-%/20-21' and a.Weaver_ClothReceipt_Code  = b.Weaver_Cloth_Receipt_Code"
            cmd.ExecuteNonQuery()

            tr.Commit()

            cn1.Close()

            MessageBox.Show("Wages IR Code Updated", "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception

            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try

    End Sub


