    Private Sub mnu_tools_Field_Check_ClothInvoice_Cloth_DeliveryCode_Updation_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_tools_Field_Check_ClothInvoice_Cloth_DeliveryCode_Updation.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim da As New SqlClient.SqlDataAdapter
        Dim da1 As New SqlClient.SqlDataAdapter
        Dim cmd As New SqlClient.SqlCommand
        Dim dt1 As New DataTable
        Dim dt2 As New DataTable
        Dim dt3 As New DataTable
        Dim Nr As Integer = 0
        Dim g As New Password
        Dim I As Long = 0, J As Long = 0, K As Long = 0
        Dim vEntDCNO As String
        Dim vEntDcCode As String
        Dim OneDcSTS As Boolean = False
        Dim vDc_InvMtrs As String = ""
        Dim vDcNoofDet As Integer = 0


        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDINV123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED.....", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1


        Try

            da = New SqlClient.SqlDataAdapter("select a.* from ClothSales_Invoice_Details a, ClothSales_Invoice_Head b Where b.Invoice_Selection_Type = 'DELIVERY' and a.ClothSales_Invoice_Code = b.ClothSales_Invoice_Code and a.ClothSales_Delivery_Code = ''", cn1)
            dt1 = New DataTable
            da.Fill(dt1)
            If dt1.Rows.Count > 0 Then

                For I = 0 To dt1.Rows.Count - 1

                    vEntDCNO = ""
                    If Trim(dt1.Rows(I).Item("Order_Delivery_No").ToString) <> "" Then

                        OneDcSTS = True
                        vEntDCNO = Trim(dt1.Rows(I).Item("Order_Delivery_No").ToString)

                        vEntDcCode = ""

                        If Trim(vEntDCNO) <> "" Then

                            da = New SqlClient.SqlDataAdapter("Select * from ClothSales_Delivery_Head Where ClothSales_Delivery_No = '" & Trim(vEntDCNO) & "' Order by ClothSales_Delivery_Code", cn1)
                            dt3 = New DataTable
                            da.Fill(dt3)
                            If dt3.Rows.Count > 0 Then
                                For J = 0 To dt3.Rows.Count - 1

                                    vEntDcCode = Trim(dt3.Rows(J).Item("ClothSales_Delivery_Code").ToString)

                                    da = New SqlClient.SqlDataAdapter("Select * from ClothSales_Delivery_Details Where ClothSales_Delivery_Code = '" & Trim(vEntDcCode) & "' Order by Sl_No", cn1)
                                    dt2 = New DataTable
                                    da.Fill(dt2)
                                    If dt2.Rows.Count > 0 Then
                                        For K = 0 To dt2.Rows.Count - 1
                                            If (Val(dt2.Rows(K).Item("Meters").ToString) - Val(dt2.Rows(K).Item("Return_Meters").ToString)) = Val(dt1.Rows(I).Item("meters").ToString) Then
                                                Nr = 0
                                                cmd.CommandText = "Update ClothSales_Invoice_Details set ClothSales_Delivery_Code = '" & Trim(dt2.Rows(K).Item("ClothSales_Delivery_Code").ToString) & "', ClothSales_Delivery_SlNo = " & Str(Val(dt2.Rows(K).Item("ClothSales_Delivery_SlNo").ToString)) & " Where ClothSales_Invoice_Code = '" & Trim(dt1.Rows(I).Item("ClothSales_Invoice_Code").ToString) & "'  and ClothSales_Invoice_SlNo = " & Str(Val(dt1.Rows(I).Item("ClothSales_Invoice_SlNo").ToString)) & " and ClothSales_Delivery_Code = ''"
                                                Nr = cmd.ExecuteNonQuery()
                                                Exit For
                                            End If
                                        Next K

                                    End If
                                    dt2.Clear()


                                Next J
                            End If
                            dt3.Clear()


                        End If

                    End If

                Next I

            End If
            dt1.Clear()

            da = New SqlClient.SqlDataAdapter("Select a.ClothSales_Delivery_code, a.meters, a.invoice_meters, a.Return_Meters, a.* from ClothSales_Delivery_Details a Where a.invoice_meters <> 0 and a.ClothSales_Delivery_code NOT IN ( SELECT Z1.ClothSales_Delivery_code FROM clothsales_invoice_details Z1 ) and a.ClothSales_Delivery_code NOT IN ( SELECT Z2.ClothSales_Delivery_code FROM Ic_Invoice_Details Z2 )", cn1)
            dt1 = New DataTable
            da.Fill(dt1)
            If dt1.Rows.Count > 0 Then

                For I = 0 To dt1.Rows.Count - 1

                    If (Val(dt1.Rows(I).Item("Meters").ToString) - Val(dt1.Rows(I).Item("Return_Meters").ToString)) = Val(dt1.Rows(I).Item("invoice_meters").ToString) Then
                        Nr = 0
                        cmd.CommandText = "Update ClothSales_Delivery_Details set invoice_meters = 0 Where ClothSales_Delivery_code = '" & Trim(dt1.Rows(I).Item("ClothSales_Delivery_code").ToString) & "' and ClothSales_Delivery_SlNo = " & Str(Val(dt1.Rows(I).Item("ClothSales_Delivery_SlNo").ToString)) & " and invoice_meters <> 0"
                        Nr = cmd.ExecuteNonQuery()
                    End If

                Next I

            End If
            dt1.Clear()

            da.Dispose()
            cn1.Close()

            MessageBox.Show("Invoice Delivery Code Successfully Updated", "FOR FIELDS UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            MessageBox.Show(ex.Message, "FOR FIELDS UPDATION.....", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally
            dt1.Dispose()
            dt2.Dispose()
            da.Dispose()

        End Try

    End Sub

