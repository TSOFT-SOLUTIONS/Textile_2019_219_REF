    Private Sub MdiMenu_VisiblitySettings(ByVal objMenuItem As ToolStripDropDownItem)
        Dim ISPREVSEPARATOR As Boolean = False

        Try

            For IntX As Integer = 0 To objMenuItem.DropDownItems.Count - 1

                If Trim(UCase(objMenuItem.DropDownItems(IntX).Tag)) = "INVISIBLE" Then
                    objMenuItem.DropDownItems(IntX).Visible = False

                Else

                    If ISPREVSEPARATOR = True And Len(Trim(objMenuItem.DropDownItems(IntX).Text)) <= 2 Then
                        objMenuItem.DropDownItems(IntX).Visible = False
                        objMenuItem.DropDownItems(IntX).Tag = "INVISIBLE"

                    Else
                        objMenuItem.DropDownItems(IntX).Visible = True

                    End If

                    If Len(Trim(objMenuItem.DropDownItems(IntX).Text)) <= 2 Then
                        ISPREVSEPARATOR = True
                    Else
                        ISPREVSEPARATOR = False
                    End If

                    'If Trim(LCase(objMenuItem.DropDownItems(IntX).Name)) = Trim(LCase("mnu_Entry_Chemical_Main")) Then
                    '    Debug.Print(objMenuItem.DropDownItems(IntX).Text)
                    'End If

                    If Len(Trim(objMenuItem.DropDownItems(IntX).Text)) > 2 Then
                        If Not (TypeOf objMenuItem.DropDownItems(IntX) Is System.Windows.Forms.ToolStripSeparator) Then
                            Dim ObjTabstripDropItem As ToolStripDropDownItem = objMenuItem.DropDownItems(IntX)
                            If ObjTabstripDropItem.HasDropDownItems Then
                                MdiMenu_VisiblitySettings(objMenuItem.DropDownItems(IntX))
                            End If
                        End If
                    End If

                End If

            Next

            MdiMenu_Numbering(objMenuItem)

        Catch ex As Exception
            '----
            MessageBox.Show(ex.Message, "MENU CREATION " & objMenuItem.Name & "...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            'Debug.Print(objMenuItem.Name)

        End Try

    End Sub


    Private Sub MdiMenu_Numbering(ByVal objMenuItem As ToolStripMenuItem)
        Dim Cnt As Integer = -1

        Try

            For IntX As Integer = 0 To objMenuItem.DropDownItems.Count - 1

                If Trim(UCase(objMenuItem.DropDownItems(IntX).Tag)) <> "INVISIBLE" Then

                    If Not (TypeOf objMenuItem.DropDownItems(IntX) Is System.Windows.Forms.ToolStripSeparator) Then

                        'If Trim(LCase(objMenuItem.DropDownItems(IntX).Name)) = Trim(LCase("mnu_Entry_Chemical_Main")) Then
                        '    Debug.Print(objMenuItem.DropDownItems(IntX).Text)
                        'End If

                        If Len(Trim(objMenuItem.DropDownItems(IntX).Text)) > 2 Then
                            If Mid(Trim(objMenuItem.DropDownItems(IntX).Text), 2, 1) = "." Then
                                objMenuItem.DropDownItems(IntX).Text = Microsoft.VisualBasic.Right(Trim(objMenuItem.DropDownItems(IntX).Text), Len(Trim(objMenuItem.DropDownItems(IntX).Text)) - 2)
                            End If
                        End If

                        If Len(Trim(objMenuItem.DropDownItems(IntX).Text)) > 3 Then
                            If Mid(Trim(objMenuItem.DropDownItems(IntX).Text), 3, 1) = "." And Mid(Trim(objMenuItem.DropDownItems(IntX).Text), 2, 1) = " " Then
                                objMenuItem.DropDownItems(IntX).Text = Microsoft.VisualBasic.Right(Trim(objMenuItem.DropDownItems(IntX).Text), Len(Trim(objMenuItem.DropDownItems(IntX).Text)) - 3)
                            End If
                        End If

                        If Len(Trim(objMenuItem.DropDownItems(IntX).Text)) > 2 Then
                            Cnt = Cnt + 1
                            If Cnt < 26 Then
                                objMenuItem.DropDownItems(IntX).Text = Chr(65 + Cnt) + ". " & Trim(objMenuItem.DropDownItems(IntX).Text)
                            ElseIf Cnt < 35 Then
                                objMenuItem.DropDownItems(IntX).Text = CStr(Cnt - 25) + ". " & Trim(objMenuItem.DropDownItems(IntX).Text)
                            End If

                            If Len(Trim(objMenuItem.DropDownItems(IntX).Text)) > 2 Then
                                Dim ObjTabstripDropItem As ToolStripDropDownItem = objMenuItem.DropDownItems(IntX)
                                If ObjTabstripDropItem.HasDropDownItems Then
                                    MdiMenu_Numbering(objMenuItem.DropDownItems(IntX))
                                End If
                            End If

                        End If

                    End If

                End If

            Next


        Catch ex As Exception
            MessageBox.Show(ex.Message, "MENU CREATION NUMBERING " & objMenuItem.Name & "...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        End Try

    End Sub

