    Private Sub mnu_Tools_FieldsCheck_BabyConeProcessing_Updation_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Tools_FieldsCheck_BabyConeProcessing_Updation.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim Nr As Long = 0

        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDKMT123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            cmd.CommandText = "Update Stock_BabyCone_Processing_Details set Delivered_Bags = 0, Delivered_Cones = 0, Delivered_Weight = 0"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "Update Stock_BabyCone_Processing_Details set Baby_Bags = 0, Baby_Cones = 0, Baby_Weight = 0"
            cmd.ExecuteNonQuery()

            Da = New SqlClient.SqlDataAdapter("select a.* from Sizing_Specification_Head a where a.Rewinding_Status = 0 and a.BabyCone_Weight <> 0  Order by a.Sizing_Specification_Date, a.For_OrderBy, a.Sizing_Specification_No", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then

                For i = 0 To Dt1.Rows.Count - 1

                    cmd.CommandText = "Update Stock_BabyCone_Processing_Details set " &
                          " Baby_Bags = " & Str(Val(Dt1.Rows(i).Item("BabyCone_bags").ToString)) & ", " &
                               " Baby_Cones = " & Str(Val(Dt1.Rows(i).Item("Total_Cones").ToString)) & ", " &
                              " Baby_Weight = " & Str(Val(Dt1.Rows(i).Item("BabyCone_Weight").ToString)) & " " &
                             " Where Reference_Code = 'SZSPC-" & Trim(Dt1.Rows(i).Item("Sizing_Specification_Code").ToString) & "' and " &
                            " Company_IdNo = " & Str(Val(Dt1.Rows(i).Item("Company_IdNo").ToString))
                    Nr = cmd.ExecuteNonQuery

                Next
            End If
            Dt1.Clear()

            Da = New SqlClient.SqlDataAdapter("select a.* from Rewinding_Delivery_Details a where a.Weight <> 0  Order by a.Rewinding_Delivery_Date, a.For_OrderBy, a.Rewinding_Delivery_No", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then

                For i = 0 To Dt1.Rows.Count - 1

                    cmd.CommandText = "Update Stock_BabyCone_Processing_Details set " &
                                " Delivered_Bags = Delivered_Bags + " & Str(Val(Dt1.Rows(i).Item("Bags").ToString)) & ", " &
                                " Delivered_Cones = Delivered_Cones + " & Str(Val(Dt1.Rows(i).Item("Cones").ToString)) & ", " &
                                " Delivered_Weight = Delivered_Weight + " & Str(Val(Dt1.Rows(i).Item("Weight").ToString)) & " " &
                                " Where Reference_Code = '" & Trim(Dt1.Rows(i).Item("Reference_Code").ToString) & "'"
                    Nr = cmd.ExecuteNonQuery

                Next
            End If
            Dt1.Clear()

            tr.Commit()

            cn1.Close()

            MessageBox.Show("BabyCone Details Updated", "FOR BABYCONE UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try
    End Sub

