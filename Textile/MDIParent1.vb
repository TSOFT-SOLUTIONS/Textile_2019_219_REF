    Private Sub FieldsCheck_CustomerWise(ByVal cn1 As SqlClient.SqlConnection)
        Dim cmd As New SqlClient.SqlCommand
        Dim da1 As New SqlClient.SqlDataAdapter
        Dim dt1 As New DataTable
        Dim CmpNm As String = ""
        Dim SurNm As String = ""

        On Error Resume Next

        cmd.Connection = cn1

        If Trim(UCase(Common_Procedures.settings.CustomerCode)) = "1019" Or Trim(UCase(Common_Procedures.settings.CustomerCode)) = "1075" Then '---- SUBHAM Textiles (Somanur)

            'cmd.CommandText = "Update " & Trim(Common_Procedures.CompanyDetailsDataBaseName) & "..CompanyGroup_Head set Transfer_To_CompanyGroupIdNo = 2 Where CompanyGroup_IdNo = 3"
            'cmd.ExecuteNonQuery()

            If Val(Common_Procedures.CompGroupIdNo) = 2 Or Val(Common_Procedures.CompGroupIdNo) = 3 Then

                CmpNm = Common_Procedures.get_FieldValue(cn1, "Company_Head", "Company_Name", "(Company_IdNo = 1)")
                SurNm = Common_Procedures.Remove_NonCharacters(CmpNm)

                cmd.CommandText = "Update ledger_head set Ledger_Name = '" & Trim(CmpNm) & "', Sur_Name = '" & Trim(SurNm) & "', Ledger_MainName = '" & Trim(CmpNm) & "' Where Ledger_IdNo = 4"
                cmd.ExecuteNonQuery()

                cmd.CommandText = "Update Ledger_AlaisHead set Ledger_DisplayName = '" & Trim(CmpNm) & "' Where Ledger_IdNo = 4"
                cmd.ExecuteNonQuery()

            End If

            'If Val(Common_Procedures.CompGroupIdNo) = 3 Then

            '    cmd.CommandText = "Update ledger_head set Transfer_To_LedgerIdNo = 276 Where Ledger_IdNo = 4"
            '    cmd.ExecuteNonQuery()

            '    cmd.CommandText = "Update ledger_head set Transfer_To_LedgerIdNo = 4 Where Ledger_IdNo = 133"
            '    cmd.ExecuteNonQuery()

            'End If

        End If

        dt1.Dispose()
        da1.Dispose()

        cmd.Dispose()

    End Sub

