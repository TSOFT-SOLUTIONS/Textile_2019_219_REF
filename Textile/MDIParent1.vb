    Private Sub mnu_Tools_Field_Check_Delete_ReportTemp_Tables_Click(sender As Object, e As EventArgs) Handles mnu_Tools_Field_Check_Delete_ReportTemp_Tables.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim da1 As SqlClient.SqlDataAdapter
        Dim dt1 As DataTable
        Dim I As Integer
        Dim vTBLname As String = ""

        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> Trim(UCase("TSDRTT3222")) Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        Try
            cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
            cn1.Open()

            cmd.Connection = cn1

            da1 = New SqlClient.SqlDataAdapter("SELECT * FROM sys.tables where type_desc = 'USER_TABLE' and (name like 'ReportTemp[_]%[_]%' or name like 'ReportTempSimple[_]%[_]%' or name like 'ReportTempSub[_]%[_]%' or name like 'TempTable1[_]%[_]%' or name like 'TempTable2[_]%[_]%' or name like 'TempTable3[_]%[_]%' or name like 'EntryTemp[_]%[_]%' or name like 'EntryTempSimple[_]%[_]%' or name like 'EntryTempSub[_]%[_]%' or name like 'TempTable_for_Closed_SalesOrders[_]%[_]%' or name like 'TempTable_for_CrossChecking_PcsBale1[_]%[_]%' or name like 'TempTable_for_CrossChecking_PcsBale2[_]%[_]%') order by name", cn1)
            dt1 = New DataTable
            da1.Fill(dt1)
            If dt1.Rows.Count > 0 Then
                For I = 0 To dt1.Rows.Count - 1
                    If IsDBNull(dt1.Rows(I).Item("name").ToString) = False Then
                        vTBLname = dt1.Rows(I).Item("name").ToString()
                        If Trim(vTBLname) <> "" Then
                            cmd.CommandText = "drop table " & Trim(vTBLname)
                            cmd.ExecuteNonQuery()
                        End If
                    End If
                Next I
            End If
            dt1.Clear()

            cn1.Close()

            MessageBox.Show("Temporary log Cleared Successfully", "FOR CLEARING TEMP LOGS...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            MessageBox.Show(ex.Message, "FOR CLEARING TEMP LOGS...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally
            cmd.Dispose()
            cn1.Dispose()

        End Try
    End Sub

