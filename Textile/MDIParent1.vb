    Private Sub mnu_Tools_Data_Transfer_StoresItem_Rate_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Tools_Data_Transfer_StoresItem_Rate.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da1 As New SqlClient.SqlDataAdapter
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim Dt As New DataTable
        Dim vItm_IdNo As Integer, vBrand_IdNo As Integer
        Dim vItm_Name As String, vSur_Name As String
        Dim vRate As Single, vReOrd_Qty As Single, vRate_Old As Single, vRate_Scrp As Single
        Dim sno As Integer


        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDTRA123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()
        cmd.Connection = cn1

        cmd.CommandText = "truncate table " & Trim(Common_Procedures.ReportTempTable) & ""
        cmd.ExecuteNonQuery()

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            vBrand_IdNo = 0
            Da = New SqlClient.SqlDataAdapter("select min(Brand_idno) from Brand_Head Where Brand_idno <> 0", cn1)
            Da.SelectCommand.Transaction = tr
            Dt = New DataTable
            Da.Fill(Dt)

            If Dt.Rows.Count > 0 Then
                If IsDBNull(Dt.Rows(0)(0).ToString) = False Then
                    vBrand_IdNo = Val(Dt.Rows(0)(0).ToString)
                End If
            End If

            If Val(vBrand_IdNo) = 0 Then
                cmd.CommandText = "Insert into Brand_Head(Brand_idno, Brand_Name, Sur_Name)" &
                    "values ( 1 , 'LOCAL',  'LOCAL')"
                cmd.ExecuteNonQuery()

                vBrand_IdNo = 1
            End If

            Dt.Dispose()
            Da.Dispose()


            Da1 = New SqlClient.SqlDataAdapter("select * from Stores_Item_Head Where Item_Idno <> '' Order by Item_Idno", cn1)
            Da1.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da1.Fill(Dt1)

            sno = 0
            If Dt1.Rows.Count > 0 Then

                For I = 0 To Dt1.Rows.Count - 1

                    If Val(Dt1.Rows(I).Item("Item_Idno").ToString) <> 0 Then

                        vItm_IdNo = Val(Dt1.Rows(I).Item("Item_IdNo").ToString)

                        vItm_Name = Replace(Dt1.Rows(I).Item("Item_Name").ToString, "'", "")

                        vSur_Name = Common_Procedures.Remove_NonCharacters(vItm_Name)

                        vRate = Val(Dt1.Rows(I).Item("Rate").ToString)
                        vReOrd_Qty = Val(Dt1.Rows(I).Item("ReOrder_Quantity").ToString)
                        vRate_Old = Val(Dt1.Rows(I).Item("Rate_Old").ToString)
                        vRate_Scrp = Val(Dt1.Rows(I).Item("Rate_Scrap").ToString)

                        sno = 1
                        ' sno = sno + 1
                        cmd.CommandText = "Insert into Stores_Item_Details ( Item_IdNo        ,            Item_Name      ,      Sur_Name         , Sl_no ,  Brand_IdNo           , Rate                   , Rate_Old               ,          Rate_Scrap       ) " &
                                            "       Values (" & Str(Val(vItm_IdNo)) & ", '" & Trim(vItm_Name) & "', '" & Trim(vSur_Name) & "'  , " & Str(Val(sno)) & "  ,  " & Str(Val(vBrand_IdNo)) & " , " & Str(Val(vRate)) & ", " & Str(Val(vRate_Old)) & ", " & Str(Val(vRate_Scrp)) & ") "
                        cmd.ExecuteNonQuery()

                    End If

                Next

            End If

            tr.Commit()

            cn1.Close()

            MessageBox.Show("TRANSFER Successfully!!!", "FOR ITEM RATE TRANSFER...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR ITEM RATE TRANSFER...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try
    End Sub

