    Private Sub mnu_tools_Fields_Check_YarnPO_Pending_Updation_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_tools_Fields_Check_YarnPO_Pending_Updation.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim Nr As Long = 0
        Dim RcptWgt As String = 0
        Dim RcptBg As String = 0
        Dim RcptCn As String = 0


        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDJPR123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            cmd.CommandText = "Update Yarn_Purchase_Order_Details Set Purchase_Bags = 0, Purchase_Cones = 0, Purchase_Weight = 0"
            cmd.ExecuteNonQuery()

            Da = New SqlClient.SqlDataAdapter("Select a.* from Yarn_Purchase_Details a where a.Weight <> 0 and a.Yarn_Purchase_Order_Code <> '' and a.Yarn_Purchase_Order_Details_Slno <> 0 Order by a.Yarn_Purchase_Date, a.For_OrderBy, a.Yarn_Purchase_No, a.Yarn_Purchase_Code", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then

                For i = 0 To Dt1.Rows.Count - 1

                    RcptBg = Val(Dt1.Rows(i).Item("Bags").ToString)
                    RcptCn = Val(Dt1.Rows(i).Item("Cones").ToString)
                    RcptWgt = Format(Val(Dt1.Rows(i).Item("Weight").ToString), "##########0.000")

                    cmd.CommandText = "Update Yarn_Purchase_Order_Details Set " &
                                        " Purchase_Bags = Purchase_Bags  + " & Str(Val(RcptBg)) & ", " &
                                        " Purchase_Cones = Purchase_Cones  + " & Str(Val(RcptCn)) & ", " &
                                        " Purchase_Weight = Purchase_Weight  + " & Str(Val(RcptWgt)) & " " &
                                        " Where Yarn_Purchase_Order_Code = '" & Trim(Dt1.Rows(i).Item("Yarn_Purchase_Order_Code").ToString) & "' and " &
                                        " Yarn_Purchase_Order_Details_SlNo = " & Str(Val(Dt1.Rows(i).Item("Yarn_Purchase_Order_Details_Slno").ToString))
                    Nr = cmd.ExecuteNonQuery

                    cmd.CommandText = "Update Yarn_Purchase_Head Set " &
                                        " Purchase_Type = 'ORDER' " &
                                        " Where Yarn_Purchase_Code = '" & Trim(Dt1.Rows(i).Item("Yarn_Purchase_Code").ToString) & "' "
                    Nr = cmd.ExecuteNonQuery

                Next

            End If
            Dt1.Clear()

            tr.Commit()

            cn1.Close()

            MessageBox.Show("Yarn PO Pending Updated", "FOR YARN PO UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR FIELDS CREATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try
    End Sub

