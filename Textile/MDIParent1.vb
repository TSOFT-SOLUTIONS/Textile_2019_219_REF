    Private Sub Get_ClothType_Value(ByVal Cn1 As SqlClient.SqlConnection)
        Dim da1 As New SqlClient.SqlDataAdapter
        Dim dt1 As New DataTable
        Dim i As Integer

        Try

            Common_Procedures.ClothType.Type1 = ""
            Common_Procedures.ClothType.Type2 = ""
            Common_Procedures.ClothType.Type3 = ""
            Common_Procedures.ClothType.Type4 = ""
            Common_Procedures.ClothType.Type5 = ""

            da1 = New SqlClient.SqlDataAdapter("select * from ClothType_Head", Cn1)
            dt1 = New DataTable
            da1.Fill(dt1)

            If dt1.Rows.Count > 0 Then
                For i = 0 To dt1.Rows.Count - 1
                    If IsDBNull(dt1.Rows(i).Item("ClothType_Name").ToString) = False Then
                        If Val(dt1.Rows(i).Item("ClothType_IdNo").ToString()) = 1 Then
                            Common_Procedures.ClothType.Type1 = dt1.Rows(i).Item("ClothType_Name").ToString()
                        ElseIf Val(dt1.Rows(i).Item("ClothType_IdNo").ToString()) = 2 Then
                            Common_Procedures.ClothType.Type2 = dt1.Rows(i).Item("ClothType_Name").ToString()
                        ElseIf Val(dt1.Rows(i).Item("ClothType_IdNo").ToString()) = 3 Then
                            Common_Procedures.ClothType.Type3 = dt1.Rows(i).Item("ClothType_Name").ToString()
                        ElseIf Val(dt1.Rows(i).Item("ClothType_IdNo").ToString()) = 4 Then
                            Common_Procedures.ClothType.Type4 = dt1.Rows(i).Item("ClothType_Name").ToString()
                        ElseIf Val(dt1.Rows(i).Item("ClothType_IdNo").ToString()) = 5 Then
                            Common_Procedures.ClothType.Type5 = dt1.Rows(i).Item("ClothType_Name").ToString()
                        End If
                    End If
                Next i
            End If

            dt1.Dispose()
            da1.Dispose()

        Catch ex As Exception

            MessageBox.Show(ex.Message, "ERROR IN GETTING SETTINGS VALUES...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        End Try

    End Sub


