    Private Sub mnu_Fields_Check_SVM_Click(ByVal sender As System.Object, ByVal e As System.EventArgs)
        Dim da As New SqlClient.SqlDataAdapter
        Dim dt As New DataTable
        Dim con As New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        Dim cmd As New SqlClient.SqlCommand
        Dim Nr As Integer = 0

        con.Open()
        cmd.Connection = con

        da = New SqlClient.SqlDataAdapter("select a.* from Voucher_Head a where (a.Voucher_Type ='Pymt' or a.Voucher_Type ='Rcpt' or a.Voucher_Type ='Cntr')  and a.Entry_Identification like 'VOUCH-%'  ", con)
        dt = New DataTable
        da.Fill(dt)

        If dt.Rows.Count > 0 Then
            For i = 0 To dt.Rows.Count - 1

                Nr = 0
                cmd.CommandText = "update Voucher_Head set Debtor_Idno = " & Val(dt.Rows(i).Item("Creditor_Idno").ToString) & " ,Creditor_Idno = " & Val(dt.Rows(i).Item("Debtor_Idno").ToString) & "  where Voucher_Code = '" & Trim(dt.Rows(i).Item("Voucher_Code").ToString) & "'"
                Nr = cmd.ExecuteNonQuery()


            Next
        End If

        dt.Dispose()
        da.Dispose()
        dt.Dispose()

        da = New SqlClient.SqlDataAdapter("select a.* from Voucher_Details a where  a.Voucher_Amount <> 0 and (a.Voucher_Type ='Pymt' or a.Voucher_Type ='Rcpt' or a.Voucher_Type ='Cntr')  and a.Entry_Identification like 'VOUCH-%'  ", con)
        dt = New DataTable
        da.Fill(dt)

        If dt.Rows.Count > 0 Then
            For i = 0 To dt.Rows.Count - 1

                If Val(dt.Rows(i).Item("Voucher_Amount").ToString) > 0 Then
                    Nr = 0
                    cmd.CommandText = "update Voucher_Details set Voucher_Amount = " & -1 * Val(dt.Rows(i).Item("Voucher_Amount").ToString) & " where Voucher_Code = '" & Trim(dt.Rows(i).Item("Voucher_Code").ToString) & "' and Ledger_Idno = " & Val(dt.Rows(i).Item("Ledger_Idno").ToString) & ""
                    Nr = cmd.ExecuteNonQuery()

                Else
                    Nr = 0
                    cmd.CommandText = "update Voucher_Details set Voucher_Amount = " & Math.Abs(Val(dt.Rows(i).Item("Voucher_Amount").ToString)) & " where Voucher_Code = '" & Trim(dt.Rows(i).Item("Voucher_Code").ToString) & "' and Ledger_Idno = " & Val(dt.Rows(i).Item("Ledger_Idno").ToString) & ""
                    Nr = cmd.ExecuteNonQuery()
                End If

                If Val(dt.Rows(i).Item("sl_no").ToString) = 1 Then
                    Nr = 0
                    cmd.CommandText = "update Voucher_Details set sl_no = 100 where Voucher_Code = '" & Trim(dt.Rows(i).Item("Voucher_Code").ToString) & "' and Ledger_Idno = " & Val(dt.Rows(i).Item("Ledger_Idno").ToString) & ""
                    Nr = cmd.ExecuteNonQuery()
                ElseIf Val(dt.Rows(i).Item("sl_no").ToString) = 2 Then
                    Nr = 0
                    cmd.CommandText = "update Voucher_Details set sl_no = 1 where Voucher_Code = '" & Trim(dt.Rows(i).Item("Voucher_Code").ToString) & "' and Ledger_Idno = " & Val(dt.Rows(i).Item("Ledger_Idno").ToString) & ""
                    Nr = cmd.ExecuteNonQuery()

                End If


            Next
        End If

        dt.Dispose()
        da.Dispose()
        dt.Dispose()

