    Private Sub mnu_Tools_FieldsCheck_Wages_Details_Updation_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnu_Tools_FieldsCheck_Wages_Details_Updation.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim Sno As Integer = 0

        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLD123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        cmd.CommandText = "truncate table " & Trim(Common_Procedures.ReportTempTable) & ""
        cmd.ExecuteNonQuery()

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            cmd.CommandText = "truncate table Weaver_Wages_Cooly_Details"
            cmd.ExecuteNonQuery()

            Da = New SqlClient.SqlDataAdapter("select a.* , b.Cloth_Pick from Weaver_Wages_Head a Left Outer Join Cloth_Head b On A.Cloth_IdNo = B.Cloth_IdNo where a.Sound_Meters <> 0 or a.Seconds_Meters <> 0  or a.Bits_Meters <> 0  or a.Reject_Meters <> 0  or a.Others_Meters <> 0  Order by a.Weaver_Wages_Date, a.for_orderby, a.Weaver_Wages_No", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then

                For i = 0 To Dt1.Rows.Count - 1

                    cmd.Parameters.Clear()
                    cmd.Parameters.AddWithValue("@WagesDate", Dt1.Rows(i).Item("Weaver_Wages_Date"))

                    If Val(Dt1.Rows(i).Item("Sound_Meters").ToString) <> 0 Then
                        Sno = Sno + 1
                        cmd.CommandText = "Insert into Weaver_Wages_Cooly_Details (                              Weaver_Wages_Code                  ,                               Company_IdNo                 ,                             Weaver_Wages_No                 ,                               for_OrderBy                 , Weaver_Wages_Date,            Sl_No     ,                                 Meters                     ,  ClothType_IdNo,        Pick                                                 ,                           Cooly                    ,                                                     Amount                                                  ) " &
                                            "      Values                         (   '" & Trim(Dt1.Rows(i).Item("Weaver_Wages_Code").ToString) & "', " & Str(Val(Dt1.Rows(i).Item("Company_IdNo").ToString)) & ", '" & Trim(Dt1.Rows(i).Item("Weaver_Wages_No").ToString) & "', " & Str(Val(Dt1.Rows(i).Item("for_OrderBy").ToString)) & ",     @WagesDate   , " & Str(Val(Sno)) & ", " & Str(Val(Dt1.Rows(i).Item("Sound_Meters").ToString)) & ",        1       ,  " & Str(Val(Dt1.Rows(i).Item("Cloth_Pick").ToString)) & "  , " & Str(Val(Dt1.Rows(i).Item("Sound_Cooly").ToString)) & ", " & Str(Val(Dt1.Rows(i).Item("Sound_Meters").ToString) * Val(Dt1.Rows(i).Item("Sound_Cooly").ToString)) & " ) "
                        cmd.ExecuteNonQuery()

                    End If

                    If Val(Dt1.Rows(i).Item("Seconds_Meters").ToString) <> 0 Then
                        Sno = Sno + 1
