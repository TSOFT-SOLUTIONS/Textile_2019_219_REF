    Private Sub mnu_Tools_FieldsCheck_Roll_Packing_Updation_Click(ByVal sender As System.Object, ByVal e As System.EventArgs)
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable

        Dim Pk_Condition As String = "RLPCK-"
        Dim vNewRollCode As String = ""
        Dim vOldRollCode As String = ""
        Dim Nr As Long = 0
        Dim PackSlpCodes As String = ""
        Dim Delv_Code As String = "", Delv_DetSlNo As Long = 0


        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDKMT123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED.....", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            Da = New SqlClient.SqlDataAdapter("select a.* from Roll_Packing_Details a", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)
            If Dt1.Rows.Count > 0 Then
                For i = 0 To Dt1.Rows.Count - 1

                    vOldRollCode = Dt1.Rows(i).Item("Roll_Code").ToString

                    vNewRollCode = Trim(Pk_Condition) & Trim(Val(Dt1.Rows(i).Item("Company_IdNo").ToString)) & "-" & Trim(Dt1.Rows(i).Item("Roll_Packing_No").ToString) & "\" & Trim(Dt1.Rows(i).Item("Roll_No").ToString) & "/" & Trim(Microsoft.VisualBasic.Right(Dt1.Rows(i).Item("Roll_Packing_Code").ToString, 5))

                    If Val(Dt1.Rows(i).Item("ClothType_IdNo").ToString) = 1 Then
                        Nr = 0
                        cmd.CommandText = "Update Weaver_ClothReceipt_Piece_Details set PackingSlip_Code_Type1 = '" & Trim(vNewRollCode) & "' Where lot_code = '" & Trim(Dt1.Rows(i).Item("Lot_Code").ToString) & "' and Piece_No = '" & Trim(Dt1.Rows(i).Item("Pcs_No").ToString) & "' and Type1_Meters <> 0 and PackingSlip_Code_Type1 = '" & Trim(vOldRollCode) & "'"
                        Nr = cmd.ExecuteNonQuery()
                        If Nr <> 1 Then
                            Throw New ApplicationException("Invalid Weaver_ClothReceipt_Piece_Details Updation - " & vNewRollCode)
                            Exit Sub
                        End If

                    ElseIf Val(Dt1.Rows(i).Item("ClothType_IdNo").ToString) = 2 Then
                        Nr = 0
                        cmd.CommandText = "Update Weaver_ClothReceipt_Piece_Details set PackingSlip_Code_Type2 = '" & Trim(vNewRollCode) & "' Where lot_code = '" & Trim(Dt1.Rows(i).Item("Lot_Code").ToString) & "' and Piece_No = '" & Trim(Dt1.Rows(i).Item("Pcs_No").ToString) & "' and Type2_Meters <> 0 and PackingSlip_Code_Type2 = '" & Trim(vOldRollCode) & "'"
                        Nr = cmd.ExecuteNonQuery()
                        If Nr <> 1 Then
                            Throw New ApplicationException("Invalid Weaver_ClothReceipt_Piece_Details Updation - " & vNewRollCode)
                            Exit Sub
                        End If

                    ElseIf Val(Dt1.Rows(i).Item("ClothType_IdNo").ToString) = 3 Then
                        Nr = 0
                        cmd.CommandText = "Update Weaver_ClothReceipt_Piece_Details set PackingSlip_Code_Type3 = '" & Trim(vNewRollCode) & "' Where lot_code = '" & Trim(Dt1.Rows(i).Item("Lot_Code").ToString) & "' and Piece_No = '" & Trim(Dt1.Rows(i).Item("Pcs_No").ToString) & "' and Type3_Meters <> 0 and PackingSlip_Code_Type3 = '" & Trim(vOldRollCode) & "'"
                        Nr = cmd.ExecuteNonQuery()
                        If Nr <> 1 Then
                            Throw New ApplicationException("Invalid Weaver_ClothReceipt_Piece_Details Updation - " & vNewRollCode)
                            Exit Sub
                        End If

                    ElseIf Val(Dt1.Rows(i).Item("ClothType_IdNo").ToString) = 4 Then
                        Nr = 0
                        cmd.CommandText = "Update Weaver_ClothReceipt_Piece_Details set PackingSlip_Code_Type4 = '" & Trim(vNewRollCode) & "' Where lot_code = '" & Trim(Dt1.Rows(i).Item("Lot_Code").ToString) & "' and Piece_No = '" & Trim(Dt1.Rows(i).Item("Pcs_No").ToString) & "' and Type4_Meters <> 0 and PackingSlip_Code_Type4 = '" & Trim(vOldRollCode) & "'"
                        Nr = cmd.ExecuteNonQuery()
                        If Nr <> 1 Then
                            Throw New ApplicationException("Invalid Weaver_ClothReceipt_Piece_Details Updation - " & vNewRollCode)
                            Exit Sub
                        End If

                    ElseIf Val(Dt1.Rows(i).Item("ClothType_IdNo").ToString) = 5 Then
                        Nr = 0
                        cmd.CommandText = "Update Weaver_ClothReceipt_Piece_Details set PackingSlip_Code_Type5 = '" & Trim(vNewRollCode) & "' Where lot_code = '" & Trim(Dt1.Rows(i).Item("Lot_Code").ToString) & "' and Piece_No = '" & Trim(Dt1.Rows(i).Item("Pcs_No").ToString) & "' and Type5_Meters <> 0 and PackingSlip_Code_Type5 = '" & Trim(vOldRollCode) & "'"
                        Nr = cmd.ExecuteNonQuery()
                        If Nr <> 1 Then
                            Throw New ApplicationException("Invalid Weaver_ClothReceipt_Piece_Details Updation - " & vNewRollCode)
                            Exit Sub
                        End If

                    End If

                    Nr = 0
                    cmd.CommandText = "Update Packing_Slip_Head set Packing_Slip_Code = '" & Trim(vNewRollCode) & "' Where Company_IdNo = " & Str(Val(Dt1.Rows(i).Item("Company_IdNo").ToString)) & " and Roll_Packing_Code = '" & Trim(Dt1.Rows(i).Item("Roll_Packing_Code").ToString) & "'  and Packing_Slip_Code = '" & Trim(vOldRollCode) & "'"
                    Nr = cmd.ExecuteNonQuery()
                    If Nr <> 1 Then
                        Throw New ApplicationException("Invalid Packing_Slip_Head Updation - " & vNewRollCode)
                        Exit Sub
                    End If

                    Nr = 0
                    cmd.CommandText = "Update Packing_Slip_Details set Packing_Slip_Code = '" & Trim(vNewRollCode) & "' Where Company_IdNo = " & Str(Val(Dt1.Rows(i).Item("Company_IdNo").ToString)) & " and Roll_Packing_Code = '" & Trim(Dt1.Rows(i).Item("Roll_Packing_Code").ToString) & "'  and Packing_Slip_Code = '" & Trim(vOldRollCode) & "'"
                    Nr = cmd.ExecuteNonQuery()
                    If Nr <> 1 Then
                        Throw New ApplicationException("Invalid Packing_Slip_Details Updation - " & vNewRollCode)
                        Exit Sub
                    End If

                    Nr = 0
                    cmd.CommandText = "Update Roll_Packing_Details Set Roll_Code = '" & Trim(vNewRollCode) & "' Where Roll_Packing_Code = '" & Trim(Dt1.Rows(i).Item("Roll_Packing_Code").ToString) & "' and Sl_No = " & Str(Val(Dt1.Rows(i).Item("Sl_No").ToString)) & " and Roll_Code = '" & Trim(vOldRollCode) & "'"
                    Nr = cmd.ExecuteNonQuery()
                    If Nr <> 1 Then
                        Throw New ApplicationException("Invalid Roll_Packing_Details Updation - " & vNewRollCode)
                        Exit Sub
                    End If

                Next

            End If
            Dt1.Clear()


            Delv_Code = ""
            Delv_DetSlNo = 0

            Da = New SqlClient.SqlDataAdapter("select a.* from Packing_Slip_Head a Where Delivery_Code <> '' Order by Delivery_Code, Delivery_DetailsSlNo", cn1)
            Da.SelectCommand.Transaction = tr
            Dt1 = New DataTable
            Da.Fill(Dt1)
            If Dt1.Rows.Count > 0 Then
                For i = 0 To Dt1.Rows.Count - 1

                    If Trim(Delv_Code) <> Trim(Dt1.Rows(i).Item("Delivery_Code").ToString) Or Val(Delv_DetSlNo) <> Val(Dt1.Rows(i).Item("Delivery_DetailsSlNo").ToString) Then
                        PackSlpCodes = ""
                    End If


                    PackSlpCodes = Trim(PackSlpCodes) & IIf(Trim(PackSlpCodes) = "", "~", "") & Trim(Dt1.Rows(i).Item("Packing_Slip_Code").ToString) & "~"

                    If Trim(UCase(Microsoft.VisualBasic.Left(Dt1.Rows(i).Item("Delivery_Code").ToString.Trim, 6))) = "CSINV-" Then
                        Nr = 0
                        cmd.CommandText = "Update ClothSales_Invoice_Details set PackingSlip_Codes = '" & Trim(PackSlpCodes) & "' Where ClothSales_Invoice_SlNo = " & Str(Val(Dt1.Rows(i).Item("Delivery_DetailsSlNo").ToString)) & " and 'CSINV-' + ClothSales_Invoice_Code = '" & Trim(Dt1.Rows(i).Item("Delivery_Code").ToString) & "'"
                        Nr = cmd.ExecuteNonQuery()
                        If Nr <> 1 Then
                            Throw New ApplicationException("Invalid ClothSales_Invoice_Details Updation - " & Dt1.Rows(i).Item("Delivery_Code").ToString)
                            Exit Sub
                        End If

                    ElseIf Trim(UCase(Microsoft.VisualBasic.Left(Dt1.Rows(i).Item("Delivery_Code").ToString.Trim, 6))) = "CSDLV-" Then
                        Nr = 0
                        cmd.CommandText = "Update ClothSales_Delivery_Details set PackingSlip_Codes = '" & Trim(PackSlpCodes) & "' Where ClothSales_Delivery_SlNo = " & Str(Val(Dt1.Rows(i).Item("Delivery_DetailsSlNo").ToString)) & " and 'CSDLV-' + ClothSales_Invoice_Code = '" & Trim(Dt1.Rows(i).Item("Delivery_Code").ToString) & "'"
                        Nr = cmd.ExecuteNonQuery()
                        If Nr <> 1 Then
                            Throw New ApplicationException("Invalid ClothSales_Delivery_Details Updation - " & Dt1.Rows(i).Item("Delivery_Code").ToString)
                            Exit Sub
                        End If

                    End If

                    Delv_Code = Trim(Dt1.Rows(i).Item("Delivery_Code").ToString)
                    Delv_DetSlNo = Val(Dt1.Rows(i).Item("Delivery_DetailsSlNo").ToString)

                Next

            End If
            Dt1.Clear()

            tr.Commit()

            cn1.Close()

            MessageBox.Show("Records Updated Successfully....", "FOR ROLL PACKING UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            tr.Rollback()
            MessageBox.Show(ex.Message, "FOR ROLL PACKING UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally
            Dt1.Dispose()
            Da.Dispose()
            cmd.Dispose()
            tr.Dispose()

        End Try

    End Sub

