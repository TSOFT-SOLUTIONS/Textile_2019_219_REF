    Private Sub mnu_Tools_FieldsCheck_Update_ClothSalesOrderNo_to_Doffing_Click(sender As Object, e As EventArgs) Handles mnu_Tools_FieldsCheck_Update_ClothSalesOrderNo_to_Doffing.Click
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim tr As SqlClient.SqlTransaction
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim Nr As Long = 0
        Dim RcptWgt As String = 0
        Dim RcptBg As String = 0
        Dim RcptCn As String = 0

        Dim g As New Password
        g.ShowDialog()

        If Trim(UCase(Common_Procedures.Password_Input)) <> "GOLDMOF123" Then
            MessageBox.Show("Invalid Password", "PASSWORD FAILED...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        tr = cn1.BeginTransaction

        Try

            cmd.Transaction = tr

            cmd.CommandText = "Update Weaver_Cloth_Receipt_Head Set ClothSales_OrderCode_forSelection = b.ClothSales_OrderCode_forSelection , Panel_Quality = 'A' from Weaver_Cloth_Receipt_Head  a , Beam_Knotting_Head b  Where a.Beam_Knotting_Code = b.Beam_Knotting_Code and (b.ClothSales_OrderCode_forSelection <> '' and b.ClothSales_OrderCode_forSelection_Quality_2 = '' and b.ClothSales_OrderCode_forSelection_Quality_3 = '' and b.ClothSales_OrderCode_forSelection_Quality_4 = '')"
            Nr = cmd.ExecuteNonQuery()
            cmd.CommandText = "Update Weaver_Cloth_Receipt_Head Set ClothSales_OrderCode_forSelection = b.ClothSales_OrderCode_forSelection , Panel_Quality = 'A' from Weaver_Cloth_Receipt_Head  a , Beam_Knotting_Head b  Where a.Beam_Knotting_Code = b.Beam_Knotting_Code and  (b.ClothSales_OrderCode_forSelection <> '' and b.ClothSales_OrderCode_forSelection_Quality_2 <> '' and b.ClothSales_OrderCode_forSelection_Quality_3 = '' and b.ClothSales_OrderCode_forSelection_Quality_4 = '') and b.ClothSales_OrderCode_forSelection = b.ClothSales_OrderCode_forSelection_Quality_2"
            Nr = cmd.ExecuteNonQuery()
            cmd.CommandText = "Update Weaver_Cloth_Receipt_Head Set ClothSales_OrderCode_forSelection = b.ClothSales_OrderCode_forSelection , Panel_Quality = 'A' from Weaver_Cloth_Receipt_Head  a , Beam_Knotting_Head b  Where a.Beam_Knotting_Code = b.Beam_Knotting_Code and (b.ClothSales_OrderCode_forSelection <> '' and b.ClothSales_OrderCode_forSelection_Quality_2 <> '' and b.ClothSales_OrderCode_forSelection_Quality_3 <> '' and b.ClothSales_OrderCode_forSelection_Quality_4 = '') and b.ClothSales_OrderCode_forSelection = b.ClothSales_OrderCode_forSelection_Quality_2 and b.ClothSales_OrderCode_forSelection = b.ClothSales_OrderCode_forSelection_Quality_3"
            Nr = cmd.ExecuteNonQuery()

            cmd.CommandText = "Update Weaver_Cloth_Receipt_Head Set ClothSales_OrderCode_forSelection = b.ClothSales_OrderCode_forSelection from Weaver_Cloth_Receipt_Head  a , Beam_Knotting_Head b  Where a.Panel_Quality <> '' and a.Panel_Quality = b.Panel_1 and a.Beam_Knotting_Code = b.Beam_Knotting_Code"
            Nr = cmd.ExecuteNonQuery()
            cmd.CommandText = "Update Weaver_Cloth_Receipt_Head Set ClothSales_OrderCode_forSelection = b.ClothSales_OrderCode_forSelection_Quality_2 from Weaver_Cloth_Receipt_Head  a , Beam_Knotting_Head b  Where a.Panel_Quality <> '' and a.Panel_Quality = b.Panel_2 and a.Beam_Knotting_Code = b.Beam_Knotting_Code"
            Nr = cmd.ExecuteNonQuery()
            cmd.CommandText = "Update Weaver_Cloth_Receipt_Head Set ClothSales_OrderCode_forSelection = b.ClothSales_OrderCode_forSelection_Quality_3 from Weaver_Cloth_Receipt_Head  a , Beam_Knotting_Head b  Where a.Panel_Quality <> '' and a.Panel_Quality = b.Panel_3 and a.Beam_Knotting_Code = b.Beam_Knotting_Code"
            Nr = cmd.ExecuteNonQuery()
            cmd.CommandText = "Update Weaver_Cloth_Receipt_Head Set ClothSales_OrderCode_forSelection = b.ClothSales_OrderCode_forSelection_Quality_4 from Weaver_Cloth_Receipt_Head  a , Beam_Knotting_Head b  Where a.Panel_Quality <> '' and a.Panel_Quality = b.Panel_4 and a.Beam_Knotting_Code = b.Beam_Knotting_Code"
            Nr = cmd.ExecuteNonQuery()

            tr.Commit()

            cn1.Close()

            MessageBox.Show("Sucessfully Updated...", "FOR FIEDS CHECK...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

