        vSQL1 = "CREATE PROCEDURE [sp_BackupDatabases]   "
        vSQL1 = vSQL1 & "            @databaseName sysname = null,   "
        vSQL1 = vSQL1 & "            @backupLocation nvarchar(max)  "
        vSQL1 = vSQL1 & "AS  "
        vSQL1 = vSQL1 & "       SET NOCOUNT ON;  "
        vSQL1 = vSQL1 & "            DECLARE @DBs TABLE "
        vSQL1 = vSQL1 & "            ( "
        vSQL1 = vSQL1 & "                  ID int IDENTITY PRIMARY KEY, "
        vSQL1 = vSQL1 & "                  DBNAME nvarchar(500) "
        vSQL1 = vSQL1 & "            ) "
        vSQL1 = vSQL1 & "             IF @databaseName<>'' "
        vSQL1 = vSQL1 & "                  BEGIN "
        vSQL1 = vSQL1 & "                        INSERT INTO @DBs (DBNAME) "
        vSQL1 = vSQL1 & "                        SELECT Name FROM master.sys.databases "
        vSQL1 = vSQL1 & "                        where state=0 "
        vSQL1 = vSQL1 & "                        AND name= ISNULL(@databaseName ,name)"
        vSQL1 = vSQL1 & "                        ORDER BY Name"
        vSQL1 = vSQL1 & "                  END"
        vSQL1 = vSQL1 & "             ELSE"
        vSQL1 = vSQL1 & "                  BEGIN "
        vSQL1 = vSQL1 & "                        INSERT INTO @DBs (DBNAME) "
        vSQL1 = vSQL1 & "                        SELECT Name FROM master.sys.databases "
        vSQL1 = vSQL1 & "                        where state=0 "
        vSQL1 = vSQL1 & "                        AND name LIKE 'tsoft%'"
        vSQL1 = vSQL1 & "                        ORDER BY Name"
        vSQL1 = vSQL1 & "                  END"
        vSQL1 = vSQL1 & "                  BEGIN "
