    Private Shared Sub Run_OE_Database_Script(ByVal Cn2 As SqlClient.SqlConnection)
        Dim cmd As New SqlClient.SqlCommand
        Dim pth As String
        Dim SqlStr As String = ""
        Dim ar() As String
        Dim fs As System.IO.FileStream
        Dim r As System.IO.StreamReader
        Dim i As Integer

        On Error Resume Next

        pth = Trim(Common_Procedures.AppPath) & "\scriptOE.SQL"

        If System.IO.File.Exists(pth) = True Then
            fs = New System.IO.FileStream(pth, System.IO.FileMode.Open)
            r = New System.IO.StreamReader(fs)
            SqlStr = r.ReadToEnd
            r.Close()
            fs.Close()
            r.Dispose()
            fs.Dispose()

        Else
            MessageBox.Show("Invalid script file (``scriptOE.SQL``)", "DOES NOT CREATE OE TABLES...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub

        End If

        If Trim(SqlStr) <> "" Then

            cmd.Connection = Cn2

            ar = Split(SqlStr, "GO")

            For i = 0 To UBound(ar)
                If Trim(ar(i)) <> "" Then

                    cmd.CommandText = Trim(ar(i))
                    cmd.ExecuteNonQuery()

                End If
            Next

        Else
            MessageBox.Show("Invalid database script", "DOES NOT CREATE OE TABLES...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub

        End If

        cmd.Dispose()

    End Sub

    Public Shared Sub FieldsCheck_Invoice_Folding_Less_Meter_Updation()
        Dim cn1 As New SqlClient.SqlConnection
        Dim da As New SqlClient.SqlDataAdapter
        Dim da1 As New SqlClient.SqlDataAdapter
        Dim cmd As New SqlClient.SqlCommand
        Dim dt As New DataTable
        Dim dt1 As New DataTable
        Dim flperc As String = 0
        Dim flmtr As String = 0
        Dim Nr As Integer = 0
        Dim g As New Password
        Dim vInvNetMtr As String = 0

        If Trim(Common_Procedures.Connection_String) = "" Then Exit Sub

        cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
        cn1.Open()

        cmd.Connection = cn1

        Try

            da = New SqlClient.SqlDataAdapter("select a.*, b.FoldingRate_Status from ClothSales_Invoice_Details a, ClothSales_Invoice_Head b Where a.ClothSales_Invoice_Code = b.ClothSales_Invoice_Code and a.InvFoldLess_Meter is Null", cn1)
            'Where InvFoldLess_Meter is Null
            dt = New DataTable
            da.Fill(dt)

            If dt.Rows.Count > 0 Then
                For i = 0 To dt.Rows.Count - 1


                    vInvNetMtr = Format(Val(dt.Rows(i).Item("Meters").ToString), "###########0.00")
                    flmtr = 0

                    If Val(dt.Rows(i).Item("FoldingRate_Status").ToString) = 0 Then

                        flperc = 100 - Val(dt.Rows(i).Item("Fold_Perc").ToString)

                        flmtr = Format(Val(dt.Rows(i).Item("Meters").ToString) * flperc / 100, "#########0.00")
                        flmtr = Math.Abs(Val(flmtr))
                        If Trim(UCase(Common_Procedures.settings.CustomerCode)) <> "1035" And Trim(UCase(Common_Procedures.settings.CustomerCode)) <> "1152" Then
                            flmtr = Common_Procedures.Meter_RoundOff(flmtr)
                        End If

                        If Val(dt.Rows(i).Item("Fold_Perc").ToString) > 100 Then
                            vInvNetMtr = Val(dt.Rows(i).Item("Meters").ToString) + Val(flmtr)
                        Else
                            vInvNetMtr = Val(dt.Rows(i).Item("Meters").ToString) - Val(flmtr)
                        End If

                    End If

                    cmd.CommandText = "Update ClothSales_Invoice_Details set InvFoldLess_Meter = " & Str(Val(flmtr)) & ", InvNet_Meter = " & Str(Val(vInvNetMtr)) & " Where ClothSales_Invoice_Code  = '" & Trim(dt.Rows(i).Item("ClothSales_Invoice_Code").ToString) & "' and ClothSales_Invoice_SlNo =  " & Str(Val(dt.Rows(i).Item("ClothSales_Invoice_SlNo").ToString))
                    cmd.ExecuteNonQuery()

                Next

            End If
            dt.Clear()
            da.Dispose()
            cn1.Close()

            'MessageBox.Show("Invoice Folding Less Meter Successfully Updated", "FOR FIELDS UPDATION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)

        Catch ex As Exception
            MessageBox.Show(ex.Message, "FOR  FieldsCheck_Invoice_Folding_Less_Meter_Updation...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)


        Finally
            dt.Dispose()
            da.Dispose()

        End Try

    End Sub

