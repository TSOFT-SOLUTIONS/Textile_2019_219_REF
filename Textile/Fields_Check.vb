        vSQL1 = vSQL1 & "                  END "
        vSQL1 = vSQL1 & "            DECLARE @BackupName nvarchar(100) "
        vSQL1 = vSQL1 & "            DECLARE @BackupFile nvarchar(300) "
        vSQL1 = vSQL1 & "            DECLARE @DBNAME nvarchar(300) "
        vSQL1 = vSQL1 & "            DECLARE @sqlCommand NVARCHAR(1000)  "
        vSQL1 = vSQL1 & "	        DECLARE @dateTime NVARCHAR(20) "
        vSQL1 = vSQL1 & "            DECLARE @Loop int                   "
        vSQL1 = vSQL1 & "            SELECT @Loop = min(ID) FROM @DBs "
        vSQL1 = vSQL1 & "      WHILE @Loop IS NOT NULL "
        vSQL1 = vSQL1 & "      BEGIN "
        vSQL1 = vSQL1 & "      SET @DBNAME = '['+(SELECT DBNAME FROM @DBs WHERE ID = @Loop)+']' "
        vSQL1 = vSQL1 & "      SET @dateTime = REPLACE(CONVERT(VARCHAR, GETDATE(),101),'/','') + '_' +  REPLACE(CONVERT(VARCHAR, GETDATE(),108),':','')   "
        vSQL1 = vSQL1 & "            SET @BackupFile = @backupLocation+REPLACE(REPLACE(@DBNAME, '[',''),']','')+ '_BackUp_FULL_'+ @dateTime+ '.BAK' "
        vSQL1 = vSQL1 & "            SET @BackupName = REPLACE(REPLACE(@DBNAME,'[',''),']','') +' full backup for '+ @dateTime "
        vSQL1 = vSQL1 & "            BEGIN "
        vSQL1 = vSQL1 & "				SET @sqlCommand = 'BACKUP DATABASE ' +@DBNAME+  ' TO DISK = '''+@BackupFile+ ''' WITH INIT, NAME= ''' +@BackupName+''', NOSKIP, NOFORMAT' "
        vSQL1 = vSQL1 & "            END "
        vSQL1 = vSQL1 & "       EXEC(@sqlCommand) "
        vSQL1 = vSQL1 & "	SELECT @Loop = min(ID) FROM @DBs where ID>@Loop "
        vSQL1 = vSQL1 & "END "

        cmd.CommandText = vSQL1
        cmd.ExecuteNonQuery()


