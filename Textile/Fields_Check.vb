        Dim cn1 As SqlClient.SqlConnection
        Dim cn2 As SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand
        Dim da2 As SqlClient.SqlDataAdapter
        Dim dt2 As DataTable
        Dim Nr As Integer
        Dim vDBName As String = ""
        Dim vConnStr As String = ""


        Try

            If Common_Procedures.settings.User_Modifications_Show_Status = 0 Then Exit Sub

            cn2 = New SqlClient.SqlConnection(Common_Procedures.ConnectionString_Master)
            cn2.Open()

            vDBName = Common_Procedures.get_UserModificationDetails_DataBaseName(Val(Common_Procedures.CompGroupIdNo))

            Try

                da2 = New SqlClient.SqlDataAdapter("Select name from sysdatabases where name = '" & Trim(vDBName) & "'", cn2)
                dt2 = New DataTable
                da2.Fill(dt2)

                Nr = 0
                If dt2.Rows.Count > 0 Then
                    If IsDBNull(dt2.Rows(0).Item("name").ToString) = False Then
                        Nr = 1
                    End If
                End If

                dt2.Dispose()
                da2.Dispose()

                If Nr = 0 Then

                    cmd.Connection = cn2
                    cmd.CommandTimeout = 1000

                    cmd.CommandText = "Create Database " & Trim(vDBName)
                    cmd.ExecuteNonQuery()

                    vConnStr = Common_Procedures.Create_Sql_ConnectionString(vDBName)


                    cn1 = New SqlClient.SqlConnection(vConnStr)
                    cn1.Open()

                    cmd.Connection = cn1
                    cmd.CommandTimeout = 1000

                    cmd.CommandText = "CREATE TABLE [User_Modification_Details] ( [Auto_SlNo] [int] IDENTITY(1,1) NOT NULL, [User_IdNo] [int] NOT NULL, [Entry_Name] [varchar](200) NOT NULL, [Entry_Type] [varchar](100) NOT NULL, [Table_Name] [varchar](200) NOT NULL, [Company_IdNo] [int] NOT NULL, [Entry_Code] [varchar](200) NOT NULL, [Modification_DateTime] [datetime] NOT NULL, [Entry_Status] [varchar](100) NOT NULL, [Modification_Details] [varchar](8000) NOT NULL , [Entry_No] [varchar](200) NULL DEFAULT (''), [for_OrderBy] [numeric](18, 2) NULL DEFAULT (0), [Details_SlNo] [int] NULL DEFAULT (0), [Field_Name] [varchar](200) NULL DEFAULT (''), [Field_NewValue] [varchar](8000) NULL DEFAULT (''), [Field_OldValue] [varchar](8000) NULL DEFAULT (''), [Modification_Status] [varchar](200) NULL DEFAULT (''), [Show_Status] [int] NULL DEFAULT (0), [Verified_Status] [int] NULL DEFAULT (0), CONSTRAINT [PK_User_Modification_Details] PRIMARY KEY CLUSTERED  ( [Auto_SlNo]  )  ON [PRIMARY] ) ON [PRIMARY] "
