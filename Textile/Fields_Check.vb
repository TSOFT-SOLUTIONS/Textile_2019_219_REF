    Public Shared Sub set_ReportTemps_Name()
        Dim vSYSNM As String
        Dim vSYSIP As String

        Try

            vSYSNM = SystemInformation.ComputerName

            If Trim(Common_Procedures.HDD_SERIALNO) = "" Then
                Common_Procedures.HDD_SERIALNO = Common_Procedures.GetDriveSerialNumber(Microsoft.VisualBasic.Left(Application.StartupPath, 2))
            End If
            vSYSIP = Common_Procedures.HDD_SERIALNO

            vSYSIP = Trim(vSYSIP) & "_" & Common_Procedures.GetIPAddress
            'If Trim(vSYSIP) = "" Then
            '    vSYSIP = Common_Procedures.GetIPAddress
            'End If

            vSYSNM = Trim(Replace(vSYSNM, ".", ""))
            vSYSNM = Trim(Replace(vSYSNM, "-", ""))
            vSYSNM = Trim(Replace(vSYSNM, "_", ""))
            vSYSNM = Trim(Common_Procedures.Remove_NonCharacters(vSYSNM))

            vSYSIP = Trim(Replace(vSYSIP, ".", ""))
            vSYSIP = Trim(Replace(vSYSIP, "-", ""))
            vSYSIP = Trim(Replace(vSYSIP, "_", ""))
            vSYSIP = Trim(Common_Procedures.Remove_NonCharacters(vSYSIP))

            Common_Procedures.ReportTempTable = "ReportTemp_" & Trim(vSYSNM) & "_" & Trim(vSYSIP)
            Common_Procedures.ReportTempSubTable = "ReportTempSub_" & Trim(vSYSNM) & "_" & Trim(vSYSIP)
            Common_Procedures.ReportTempSimpleTable = "ReportTempSimple_" & Trim(vSYSNM) & "_" & Trim(vSYSIP)
            Common_Procedures.EntryTempTable = "EntryTemp_" & Trim(vSYSNM) & "_" & Trim(vSYSIP)
            Common_Procedures.EntryTempSubTable = "EntryTempSub_" & Trim(vSYSNM) & "_" & Trim(vSYSIP)
            Common_Procedures.EntryTempSimpleTable = "EntryTempSimple_" & Trim(vSYSNM) & "_" & Trim(vSYSIP)

            Common_Procedures.TempTable1 = "TempTable1_" & Trim(vSYSNM) & "_" & Trim(vSYSIP)
            Common_Procedures.TempTable2 = "TempTable2_" & Trim(vSYSNM) & "_" & Trim(vSYSIP)
            Common_Procedures.TempTable3 = "TempTable3_" & Trim(vSYSNM) & "_" & Trim(vSYSIP)
            Common_Procedures.TempTable_for_CrossChecking_PcsBale1 = "TempTable_for_CrossChecking_PcsBale1_" & Trim(vSYSNM) & "_" & Trim(vSYSIP)
            Common_Procedures.TempTable_for_Closed_SalesOrders = "TempTable_for_Closed_SalesOrders_" & Trim(vSYSNM) & "_" & Trim(vSYSIP)

        Catch ex As Exception

            Common_Procedures.ReportTempTable = "ReportTemp"
            Common_Procedures.ReportTempSubTable = "ReportTempSub"
            Common_Procedures.ReportTempSimpleTable = "ReportTempSimple"
            Common_Procedures.EntryTempTable = "EntryTemp"
            Common_Procedures.EntryTempSubTable = "EntryTempSub"
            Common_Procedures.EntryTempSimpleTable = "EntryTemp_Simple"
            Common_Procedures.TempTable1 = "TempTable1"
            Common_Procedures.TempTable2 = "TempTable2"
            Common_Procedures.TempTable3 = "TempTable3"

            Common_Procedures.TempTable_for_CrossChecking_PcsBale1 = "TempTable_for_CrossChecking_PcsBale1"
            Common_Procedures.TempTable_for_Closed_SalesOrders = "TempTable_for_Closed_SalesOrders"

        End Try

    End Sub

    Public Shared Function Check_is_ReportTemps_Table_EXISTS(ByVal vTABLENAME As String) As Boolean
        Dim cn1 As New SqlClient.SqlConnection
        Dim cmd As New SqlClient.SqlCommand

        Try

            cn1 = New SqlClient.SqlConnection(Common_Procedures.Connection_String)
            cn1.Open()

            cmd.Connection = cn1
            cmd.CommandTimeout = 1000

            cmd.CommandText = "Select * from " & Trim(Common_Procedures.ReportTempTable)
            cmd.ExecuteNonQuery()

            cn1.Close()

            Return True

        Catch ex As Exception
            Return False

        Finally
            cn1.Dispose()
            cmd.Dispose()

        End Try

    End Function

