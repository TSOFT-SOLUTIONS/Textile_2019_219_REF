            Pk_Condition = "NCSIN-"
            Pk_Condition2 = "NCSAG-"
            Pk_Condition3 = "NCSFR-"
            Pk_Condition4 = "NCSAT-"

        ElseIf Val(vEntry_Cloth_No) = 2 Then
            If Trim(UCase(Common_Procedures.settings.CustomerCode)) = "1116" Then
                Label1.Text = " CLOTH INVOICE - GREY"
                Label1.Tag = Val(vEntry_Cloth_No)
            Else
                Label1.Text = " CLOTH INVOICE"
                Label1.Tag = Val(vEntry_Cloth_No)
            End If

            Pk_Condition = "CSINV-"
            Pk_Condition2 = "CSAGC-"
            Pk_Condition3 = "CSFRG-"
            Pk_Condition4 = "CSATD-"
        End If

        If Trim(UCase(Common_Procedures.settings.CustomerCode)) = "1087" Then
            chk_No_Folding.Visible = False
            btn_Lc_details.Visible = True
            btn_Cloth_details.Visible = True
            Cbo_RateFor.Visible = False
