                            flperc = 100 - Val(prn_DetDt.Rows(prn_DetIndx).Item("Fold_Perc").ToString)

                            flmtr = Format(Val(prn_DetDt.Rows(prn_DetIndx).Item("Meters").ToString) * flperc / 100, "#########0.00")

