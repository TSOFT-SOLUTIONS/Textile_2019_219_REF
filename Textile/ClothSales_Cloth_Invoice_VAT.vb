    Public Sub delete_record() Implements Interface_MDIActions.delete_record
        Dim Da As New SqlClient.SqlDataAdapter
        Dim Dt1 As New DataTable
        Dim cmd As New SqlClient.SqlCommand
        Dim trans As SqlClient.SqlTransaction
        Dim NewCode As String = ""
        Dim nr As Integer = 0
        If Common_Procedures.UserRight_NEWCheck(Common_Procedures.UserRightsCheckFor.DeletingEntry, Common_Procedures.UR.ClothSales_Cloth_Invoice_Entry_vat, New_Entry, Me, Con, "ClothSales_Invoice_Head", "ClothSales_Invoice_Code", NewCode, "ClothSales_Invoice_Date", "(ClothSales_Invoice_Code = '" & Trim(NewCode) & "')") = False Then Exit Sub

        If Val(lbl_Company.Tag) = 0 Then
            MessageBox.Show("Invalid Company Selection", "DOES NOT DELETE...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If

        If pnl_Back.Enabled = False Then
            MessageBox.Show("Close Other Windows", "DOES NOT DELETE...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
            Exit Sub
        End If
        If DeleteAll_STS <> True Then

            If MessageBox.Show("Do you want to Delete?", "FOR DELETION...", MessageBoxButtons.YesNoCancel, MessageBoxIcon.Question, MessageBoxDefaultButton.Button2) <> Windows.Forms.DialogResult.Yes Then
            Exit Sub
        End If

            If New_Entry = True Then
                MessageBox.Show("This is New Entry", "DOES NOT DELETE...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
                Exit Sub
            End If
        End If

        If Label1.Tag = 1 Then
            NewCode = Trim(Pk_Condition_Sarees) & Trim(Val(lbl_Company.Tag)) & "-" & Trim(lbl_InvNo.Text) & "/" & Trim(Common_Procedures.FnYearCode)
        Else
            NewCode = Trim(Val(lbl_Company.Tag)) & "-" & Trim(lbl_InvNo.Text) & "/" & Trim(Common_Procedures.FnYearCode)
        End If

        If DeleteAll_STS <> True Then

            Da = New SqlClient.SqlDataAdapter("select * from ClothSales_Invoice_Head Where Company_IdNo = " & Str(Val(lbl_Company.Tag)) & " and ClothSales_Invoice_Code = '" & Trim(NewCode) & "'", con)
        Dt1 = New DataTable
        Da.Fill(Dt1)

            If Dt1.Rows.Count > 0 Then
                If IsDBNull(Dt1.Rows(0).Item("Gate_Pass_Code").ToString) = False Then
                    If Trim(Dt1.Rows(0).Item("Gate_Pass_Code").ToString) <> "" Then
                        MessageBox.Show("Already Piece Delivered", "DOES NOT DELETE...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)
                        Exit Sub
                    End If
                End If
            End If
        End If

        Dt1.Clear()
        Dt1.Dispose()
        Da.Dispose()
        trans = con.BeginTransaction

        Try

            If Label1.Tag = 1 Then
                NewCode = Trim(Pk_Condition_Sarees) & Trim(Val(lbl_Company.Tag)) & "-" & Trim(lbl_InvNo.Text) & "/" & Trim(Common_Procedures.FnYearCode)
            Else
                NewCode = Trim(Val(lbl_Company.Tag)) & "-" & Trim(lbl_InvNo.Text) & "/" & Trim(Common_Procedures.FnYearCode)
            End If

            cmd.Connection = con
            cmd.Transaction = trans

            If Common_Procedures.VoucherBill_Deletion(con, Trim(Pk_Condition) & Trim(NewCode), trans) = False Then
                Throw New ApplicationException("Error on Voucher Bill Deletion")
            End If

            Common_Procedures.Voucher_Deletion(con, Val(lbl_Company.Tag), Trim(Pk_Condition) & Trim(NewCode), trans)
            Common_Procedures.Voucher_Deletion(con, Val(lbl_Company.Tag), Trim(Pk_Condition2) & Trim(NewCode), trans)
            Common_Procedures.Voucher_Deletion(Con, Val(lbl_Company.Tag), Trim(Pk_Condition3) & Trim(NewCode), trans)
            Common_Procedures.Voucher_Deletion(Con, Val(lbl_Company.Tag), Trim(Pk_Condition4) & Trim(NewCode), trans)

            Common_Procedures.Voucher_Deletion(Con, Val(lbl_Company.Tag), Trim(Old_Pk_Condition3) & Trim(NewCode), trans)
            Common_Procedures.Voucher_Deletion(Con, Val(lbl_Company.Tag), Trim(Old_Pk_Condition4) & Trim(NewCode), trans)

            cmd.CommandText = "Update Buyer_Offer_Head set ClothSales_Invoice_Code = '' where ClothSales_Invoice_Code = '" & Trim(NewCode) & "'"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "Update Packing_Slip_Head set Delivery_Code = '', Delivery_No = '', Delivery_DetailsSlNo = 0, Delivery_Increment = Delivery_Increment - 1, Delivery_Date = Null Where Delivery_Code = '" & Trim(Pk_Condition) & Trim(NewCode) & "'"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "Update ClothSales_order_Details set Invoice_Meters = a.Invoice_Meters - b.Meters from ClothSales_order_Details a, ClothSales_Invoice_Details b Where b.ClothSales_Invoice_Code = '" & Trim(NewCode) & "' and b.Invoice_Selection_Type = 'ORDER' and a.ClothSales_Order_code = b.ClothSales_Order_code and a.ClothSales_Order_SlNo = b.ClothSales_Order_SlNo"
            cmd.ExecuteNonQuery()

            If Trim(UCase(Common_Procedures.settings.CustomerCode)) = "1044" Then

                cmd.CommandText = "Update ClothSales_Delivery_Details set Invoice_Meters = a.Invoice_Meters - (b.Fold_Meter + b.Fold_Short_Meter) from ClothSales_Delivery_Details a, ClothSales_Invoice_Details b Where b.ClothSales_Invoice_Code = '" & Trim(NewCode) & "' and b.Invoice_Selection_Type = 'DELIVERY' and a.ClothSales_Delivery_code = b.ClothSales_Delivery_code and a.ClothSales_Delivery_SlNo = b.ClothSales_Delivery_SlNo"
                nr = cmd.ExecuteNonQuery()

            Else

                cmd.CommandText = "Update ClothSales_Delivery_Details set Invoice_Meters = a.Invoice_Meters - (b.Meters + b.Short_Meters) from ClothSales_Delivery_Details a, ClothSales_Invoice_Details b Where b.ClothSales_Invoice_Code = '" & Trim(NewCode) & "' and b.Invoice_Selection_Type = 'DELIVERY' and a.ClothSales_Delivery_code = b.ClothSales_Delivery_code and a.ClothSales_Delivery_SlNo = b.ClothSales_Delivery_SlNo"
                nr = cmd.ExecuteNonQuery()

            End If

            cmd.CommandText = "Delete from Stock_Cloth_Processing_Details Where Company_IdNo = " & Str(Val(lbl_Company.Tag)) & " and Reference_Code = '" & Trim(Pk_Condition) & Trim(NewCode) & "'"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "delete from AgentCommission_Processing_Details where company_idno = " & Str(Val(lbl_Company.Tag)) & " and Reference_Code = '" & Trim(Pk_Condition) & Trim(NewCode) & "'"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "Delete from ClothSales_Invoice_BaleEntry_Details Where Company_IdNo = " & Str(Val(lbl_Company.Tag)) & " and ClothSales_Invoice_Code = '" & Trim(NewCode) & "'"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "Delete from ClothSales_Invoice_Buyer_Offer_Details Where Company_IdNo = " & Str(Val(lbl_Company.Tag)) & " and ClothSales_Invoice_Code = '" & Trim(NewCode) & "'"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "delete from ClothSales_Invoice_Details where company_idno = " & Str(Val(lbl_Company.Tag)) & " and ClothSales_Invoice_Code = '" & Trim(NewCode) & "'"
            cmd.ExecuteNonQuery()

            cmd.CommandText = "delete from ClothSales_Invoice_Head where company_idno = " & Str(Val(lbl_Company.Tag)) & " and ClothSales_Invoice_Code = '" & Trim(NewCode) & "'"
            cmd.ExecuteNonQuery()

            trans.Commit()

            If DeleteAll_STS <> True Then

                new_record()

                MessageBox.Show("Deleted Sucessfully!!!", "FOR DELETION...", MessageBoxButtons.OKCancel, MessageBoxIcon.Information)
            End If
        Catch ex As Exception
            trans.Rollback()
            MessageBox.Show(ex.Message, "DOES NOT DELETE...", MessageBoxButtons.OKCancel, MessageBoxIcon.Error)

        Finally

            Dt1.Dispose()
            Da.Dispose()
            trans.Dispose()
            cmd.Dispose()

            If Trim(UCase(Common_Procedures.settings.CustomerCode)) = "1050" Then '---- Kumaravel Textiles (Palladam)
                If txt_InvoicePrefixNo.Enabled And txt_InvoicePrefixNo.Visible Then txt_InvoicePrefixNo.Focus()
            Else

                If msk_Date.Enabled And msk_Date.Visible Then msk_Date.Focus()
                'If dtp_Date.Enabled And dtp_Date.Visible Then dtp_Date.Focus()
            End If

        End Try

    End Sub

