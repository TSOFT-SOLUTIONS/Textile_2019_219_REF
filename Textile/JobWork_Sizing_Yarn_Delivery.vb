        ' If Val(Common_Procedures.User.IdNo) <> 1 And InStr(Common_Procedures.UR.JobWork_Sizing_Yarn_Delivery, "~L~") = 0 And InStr(Common_Procedures.UR.JobWork_Sizing_Yarn_Delivery, "~I~") = 0 Then MessageBox.Show("You have No Rights to Insert", "DOES NOT INSERT...", MessageBoxButtons.OK, MessageBoxIcon.Error) : Exit Sub

